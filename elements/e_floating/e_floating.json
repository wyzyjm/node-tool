      "media": "screen and (max-width:768px)",
      "cssText": ".e_floating .p_contrast {\r\n    border-radius: 25px;\r\n    position: fixed;\r\n    bottom: 10%;\r\n    left: 0;\r\n    z-index: 20;\r\n}\r\n\r\n.e_floating .p_contrast,\r\n.e_floating .p_contrast_hidden {\r\n    display: block;\r\n    opacity: 1;\r\n}\r\n.e_floating .p_obscuration {\r\n    height: 100%;\r\n    top: 0;\r\n    bottom: 0;\r\n    right: 0;\r\n    left: 0;\r\n    background: rgba(0, 0, 0, 0.6);\r\n    display: none;\r\n}\r\n.e_floating .p_contrast_list {\r\n    width: 100%;\r\n    position: absolute;\r\n    bottom: -100%;\r\n    transition: all 1s;\r\n}\r\n.e_floating .p_obscuration.show .p_contrast_list {\r\n    width: 100%;\r\n    position: absolute;\r\n    bottom: 0;\r\n    transition: all 1s;\r\n}\r\n.e_floating .p_tab {\r\n    display: none;\r\n}\r\n.e_floating .p_form-control {\r\n    display: block;\r\n}\r\n.e_floating .p_data_list {\r\n    flex-direction: column;\r\n    border: none;\r\n}\r\n.e_floating .p_item {\r\n    border-right: none;\r\n    border-bottom: 1px solid #ddd;\r\n}\r\n.e_floating .p_last_item {\r\n    width: 100%;\r\n    border-right: none;\r\n    flex-direction: row-reverse;\r\n    justify-content: space-between;\r\n}\r\n.e_floating .p_item_delete {\r\n    display: block;\r\n}\r\n"
