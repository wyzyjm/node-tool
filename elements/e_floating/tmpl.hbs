                        {{/if}}
                        {{#if prop.enquiry.status}}
                        <span class="px-3 py-2 p_title_compareTheBar">{{i18n.enquiry}}</span>
                        {{/if}}
                    </div>
                    <!--MO tab栏-->
                    <select class="mx-3 my-2 form-control j_mo_tab p_form-control">
                        <option value="0">{{i18n.contrast}}</option>
                        <option value="1">{{i18n.online}}</option>
                        <option value="2">{{i18n.apply}}</option>
                        <option value="3">{{i18n.sample}}</option>
                        <option value="4">{{i18n.enquiry}}</option>
                    </select>
                    <span class="px-3 py-2 p_title_hidden">{{i18n.hidden}}</span>
                </div>

