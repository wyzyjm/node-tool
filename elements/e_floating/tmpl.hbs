                <div class="alert alert-danger p_danger hide">
                    <span class="j_column"></span>{{i18n.tip}}
                </div>
                <!--tab区-->
                <div class="text-primary p_title">
                    <!--PC tab栏-->
                    <div class="j_tab p_tab">
                        {{#if prop.contrast.status}}
                        <span class="px-3 py-2 p_title_compareTheBar active">{{i18n.contrast}}</span>
                        {{/if}}
                        {{#if prop.online.status}}
                        <span class="px-3 py-2 p_title_compareTheBar">{{i18n.online}}</span>
                        {{/if}}
                        {{#if prop.apply.status}}
