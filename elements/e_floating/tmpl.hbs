                            <li class="border-right border-secondary p-4 p_item">
                                <!-- 无内容 -->
                                <a href="javascript:;"
                                   class="text-secondary bg-light mr-3 p_item_img">1</a>
                                <div class="p_nocomparedata">{{i18n.noncontrastingDescription}}
                                </div>
                            </li>
