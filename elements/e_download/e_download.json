{
  "cname": "文件下载",
  "styleId": "e_download",
  "styleClass": "",
  "tmpl": "<div class=\"${elemId} ${styleClass}\" needjs=\"true\">\r\n    <ul class=\"p_download\">\r\n        {{#each ${attachment}}}\r\n            <li class=\"d-flex align-items-center border mb-2 pl-3 pr-3 p_item\" data-title=\"{{title}}\" data-extName=\"{{extName}}\"> \r\n                <!-- 复选框 -->\r\n                <div class=\"form-check  p_name\">\r\n                    <input class=\"form-check-input p_inputitem\" type=\"checkbox\" value=\"{{{lazyFile url}}}\" id=\"checkId{{@index}}\">\r\n                    <!-- 文件名称 -->\r\n                    <label class=\"form-check-label text-nowrap text-dark p_filename\"  for=\"checkId{{@index}}\"> {{name}} </label>\r\n                </div>\r\n                <!-- 文件大小 -->\r\n                <p class=\"flex1 flex-shinrk-0 \">\r\n                    <span class=\"text-secondary p_label\"> {{../${i18n.fileSize}}} </span>\r\n                    <span> {{size}} </span>\r\n                </p>\r\n                \r\n                <!-- 按钮组 -->\r\n                <div class=\"flex1 flex-shinrk-0 text-right  p_btnGroup\">\r\n                    {{#eq extName 'pdf'}}\r\n                        <button type=\"button\" class=\"btn btn-outline-primary btn-sm pl-3 pr-3 s_button3 p_preview\" >{{../${i18n.preview}}}</button>\r\n                    {{/eq}}\r\n                    <button type=\"button\" class=\"btn btn-primary btn-sm pl-3 pr-3 s_button1 p_downbtn\">{{../${i18n.download}}}</button>\r\n                </div>\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n    <div class=\"d-flex align-items-center pl-3 pr-3 \">\r\n         <!-- 全选 -->\r\n        <div class=\"form-check p_allSelect mr-3\">\r\n            <input class=\"form-check-input p_allCheck\" type=\"checkbox\" value=\"\" id=\"allcheck\">\r\n            <label class=\"form-check-label text-nowrap text-dark p_filename\"  for=\"allcheck\"> {{${i18n.allSelect}}} </label>\r\n        </div>\r\n        <!-- 全选 -->\r\n        <button class=\"btn btn-primary btn-sm pl-3 pr-3 s_button1 p_allBtn\"> {{${i18n.download}}} </button>\r\n    </div>\r\n</div>",
  "css": [
    {
      "media": "",
      "cssText": ".e_download button{\n    font-size: 12px;\n}\n.e_download .p_item {\n    height: 42px;\n}\n.e_download .p_name {\n    width: 60%;\n    height: 30px;\n    line-height: 30px;\n    overflow: hidden;\n}\n.e_download .form-check-input {\n    margin-top: 8px;\n}\n.e_download .p_filename {\n   width: 100%;\n   text-overflow: ellipsis;\n   overflow: hidden;\n}\n.e_download .p_btnGroup {\n    white-space: nowrap;\n}\n.e_download .flex1 {\n    flex:1;\n}\n.e_download .flex2 {\n    flex:2;\n}\n.e_download .p_preview {\n   border-width: 1px; \n}\n.e_download .p_allSelect {\n    height: 30px;\n    line-height: 30px;\n}"
    },
