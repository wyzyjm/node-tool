{
  "cname": "图片型",
  "styleId": "e_uploadImg",
  "styleClass": "s_form1",
  "type": "form",
  "tmpl": "<div class=\"${elemId} ${styleClass}\" needjs=\"true\" {{#if ${validate}.required.value}}required{{/if}}>\n    {{#if ${showLabel}}}\n    <label class=\"p_label\">\n        <span class=\"s_label\">{{${label}}}</span>\n    </label>\n    {{/if}}\n    <div class=\"p_imgList clearfix\">\n        <div class=\"p_imgItem p_uploadBtn\">\n            <div>\n                <input type=\"file\" name=\"file\" accept=\"image/*\" class=\"uploadInput\" multiple>\n                {{{${upIcon}}}}\n                <p class=\"text-danger\">{{${i18n.upload}}}</p>\n            </div>\n        </div>\n        <ul class=\"p_uploadList\">\n            \n        </ul>\n    </div>\n    <p class=\"p_placeholder s_other\">{{${placeholder}}}</p>\n    <div class=\"p_initList clearfix\"></div>\n    <input name=\"${elemId}\" class=\"p_input\" type=\"hidden\" >\n    <div class=\"invalid-feedback\"></div>\n</div>",
  "css": [
    {
      "media": "",
      "cssText": ".e_uploadImg .p_imgItem{\n    width: 190px;\n    height: 130px;\n    margin-right: 20px;\n    margin-bottom: 20px;\n    float: left;\n    text-align: center;\n    position: relative;\n    border: 1px solid #e0e0e0;\n    border-radius: 4px;\n}\n.e_uploadImg .p_imgItem img{\n    width: 100%;\n    height: 100%;\n    object-fit: contain;\n}\n.e_uploadImg .p_uploadBtn{\n    background-color: #f0f0f0;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    cursor: pointer;\n    position: relative;\n}\n.e_uploadImg .p_uploadBtn input {\n    position: absolute;\n    right: 0;\n    top: 0;\n    opacity: 0;\n    cursor: pointer;\n    width: 100%;\n    height: 100%;\n    z-index: 2;\n}\n.e_uploadImg  .p_placeholder{\n    color: #999;\n    margin: 0;\n}\n.e_uploadImg .p_imgOp,.e_uploadImg .p_imgStatus{\n    position: absolute;\n    left: 0;\n    top: 0;\n    width: 100%;\n    height: 100%;\n    background-color: rgba(0, 0, 0, 0.5);\n    color: #fff;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    display: none;\n    z-index: 3;\n}\n.e_uploadImg .p_imgItem.onLoad .p_imgStatus{\n    display: flex;\n}\n.e_uploadImg .p_imgItem:hover .p_imgOp{\n    display: flex;\n}\n.e_uploadImg .icon{\n    width: 30px;\n    height: 30px;\n}\n.e_uploadImg .p_imgItem.onLoad:hover .p_imgOp{\n    display: none;\n}\n.e_uploadImg .p_imgName{\n    word-break: break-all;\n    line-height: 20px;\n    padding: 0 5px;\n    max-height: 60px;\n    overflow: hidden;\n}\n.e_uploadImg .p_initList{\n    padding-top: 10px;\n}\n.e_uploadImg .p_fileItem{\n    margin-bottom: 10px;\n    overflow: hidden;\n}"
    },
