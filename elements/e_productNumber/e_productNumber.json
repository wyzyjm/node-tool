{
  "cname": "产品详情数量",
  "styleId": "e_productNumber",
  "styleClass": "",
  "tmpl": "<div class=\"${elemId} ${styleClass}\" needjs=\"true\">\n    <!-- lable -->\n    <div class=\"p_label\">{{${i18n.count}}}</div>\n    <div class=\"p_box\">\n        <!-- 减少 -->\n        <div class=\"border rounded-left p_reduce\" data-moq=\"{{${moq}}}\">-</div>\n        <!-- input -->\n        <input type=\"text\" class=\"border p_input\" value=\"{{#if ${moq}}} {{${moq}}} {{^}} 1 {{/if}}\">\n        <!-- 增加 -->\n        <div class=\"border rounded-right p_increase\" data-stock=\"{{${stock}}}\">+</div>\n    </div>\n</div>",
  "css": [
    {
      "media": "",
      "cssText": ".e_productNumber{\n    display: flex;\n    align-items: center;\n}\n.e_productNumber :focus {\n    outline: 0;\n}\n.e_productNumber .p_label {\n    flex-grow: 0;\n    flex-shrink: 0;\n    margin-right: 10px;\n}\n.e_productNumber .p_box {\n    display: flex;\n    float: left;\n}\n\n.e_productNumber .p_reduce{\n    width: 28px;\n    height: 28px;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    flex-shrink: 0;\n    flex-grow: 0;\n    border-right: none;\n    cursor: pointer;\n}\n.e_productNumber .p_input{\n    width: 40px;\n    height: 28px;\n    text-align: center;\n}\n.e_productNumber .p_increase{\n    width: 28px;\n    height: 28px;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    flex-shrink: 0;\n    flex-grow: 0;\n    border-left: none;\n    cursor: pointer;\n}"
    },
    {
      "media": "screen and (max-width:768px)",
      "cssText": ""
    },
    {
      "media": "screen and (min-width: 769px)",
      "cssText": ""
    },
    {
      "media": "screen and (max-width: 1024px) and (min-width: 769px)",
      "cssText": ""
    }
  ],
