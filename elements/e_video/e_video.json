{
  "cname": "视频",
  "styleId": "e_video",
  "styleClass": "s_img",
  "tmpl": "<div class=\"video ${elemId} ${styleClass} p_video\" needjs=\"true\">\n    <span class=\"cover\">\n        <img class=\"playBtn\" src=\"/public/img/playVideo.png\" alt=\"\">\n        {{#if ${coverImage}}}\n            <img class=\"coverImage\" src=\"{{${coverImage}}}\" alt=\"\">\n        {{/if}}\n    </span>\n    <video class=\"video\" src=\"{{${video}}}\" muted></video>\n    <input type=\"hidden\" value='{{{propJson}}}' name=\"propJson\" />\n</div>",
  "css": [
    {
      "media": "",
      "cssText": ".e_video{\n    width: 100%;\n    position: relative;\n    display: inline-block;\n}\n.e_video video{\n    width: 100%;\n    display: block;\n}\n.e_video .cover{\n    width: 100%;\n    height: 100%;\n    position: absolute;\n    left: 0;\n    top: 0;\n}\n.e_video .cover .coverImage{\n    width: 100%;\n    height: 100%;\n    object-fit: contain;\n}\n.e_video .cover .playBtn{\n    position:absolute;\n    width: 80px;\n    height: 80px;\n    z-index: 2;\n    left: 50%;\n    top: 50%;\n    margin: -40px auto auto -40px;\n    cursor: pointer;\n}"
    },
    {
      "media": "screen and (max-width:768px)",
      "cssText": ""
    },
    {
      "media": "screen and (min-width: 769px)",
      "cssText": ""
    },
    {
      "media": "screen and (max-width: 1024px) and (min-width: 769px)",
      "cssText": ""
    }
  ],
