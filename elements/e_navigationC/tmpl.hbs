<div class="e_navigationC" needjs="true">
    {{#if prop.dataList}}
    <p class="p_iconBox">
        {{{prop.menu}}}
        {{{prop.close}}}
    </p>
    <ul class="p_navBox1">
        {{#each prop.dataList}}
        <li class="border-bottom p_navItem1">
            <p class="p_navCon">
                <a href="{{#if url}}{{url}}{{^}}javascript:;{{/if}}" {{#if url}}target="{{#eq target 'blank'}}_blank{{/eq}}"{{/if}}>
                    <span>{{navName}}</span>
                    {{#if children}}{{{../prop.arrow}}}{{/if}}
                </a>
            </p>
            {{#if children}}
            <div class="bg-white p_navBox2">
                <ul>
                    {{#each children}}
                    <li class="border p_navItem2">
                        <p>
                            <a href="{{#if url}}{{url}}{{^}}javascript:;{{/if}}" {{#if url}}target="{{#eq target 'blank'}}_blank{{/eq}}"{{/if}}>
                                <span>{{navName}}</span>
                            </a>
                        </p>
                    </li>
                    {{/each}}
                </ul>
                <div class="p_imgBox">
                    {{#each children}}
                        <span class="p_img">{{#if img}}{{{nLazyImage img navName 'true'}}}{{/if}}</span>
                    {{/each}}
                </div>
            </div>
            {{/if}}
        </li>
        {{/each}}
    </ul>
    {{^}}
    {{{nNoDataPrompt i18n.noData}}}
    {{/if}}
</div>