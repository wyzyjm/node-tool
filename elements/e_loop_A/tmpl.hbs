<div class="e_loop_A s_list" needjs="true">
    <div class="swiper-container">
        {{#arrControl prop.dataList prop.pageConfig.status}}
            <div class="swiper-wrapper">
                {{#prop.dataList}}
                    <div class="swiper-slide">
                        <div class="cbox-${id} p_template"></div>
                    </div>
                {{/prop.dataList}}
            </div>
            <!-- 如果需要导航按钮 -->
            <div class="js_editor_click swiper-button-prev"></div>
            <div class="js_editor_click swiper-button-next"></div>
        {{else}}
            <div class="cbox-${id}-1 p_nodata"></div>
        {{/arrControl}}
    </div>
</div>