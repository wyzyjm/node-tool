{
  "cname": "循环体容器",
  "styleId": "e_loop",
  "styleClass": "s_list",
  "tmpl": "<div class=\"${elemId} ${styleClass}\">\r\n    {{#if ${dataList}}}\r\n        <div class=\"p_list\">\r\n        {{#${dataList}}}\r\n            <div class=\"cbox-${id} p_item\"></div>\r\n        {{/${dataList}}}\r\n        </div>\r\n        <div class=\"p_page\">\r\n            {{{nPaging ${page} ${pageConfig}}}}\r\n        </div>\r\n    {{^}}\r\n    {{{nNoDataPrompt}}}\r\n    {{/if}}\r\n</div>\r\n",
  "css": [
    {
      "media": "",
      "cssText": ".e_loop .p_list{\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n}\r\n.e_loop .p_item{\r\n    flex:0 0 33.3%;\r\n    position: relative;\r\n}\r\n.e_loop .p_page{\r\n    text-align: center;\r\n}"
    },
    {
      "media": "screen and (max-width:768px)",
      "cssText": ""
    },
    {
      "media": "screen and (min-width: 769px)",
      "cssText": ""
    },
    {
      "media": "screen and (max-width: 1024px) and (min-width: 769px)",
      "cssText": ""
    }
  ],
  "prop": {
    "dataList": [
      {},
      {},
      {},
      {},
      {},
      {}
    ],
    "page": {
      "from": 0,
      "size": 6,
      "totalCount": 100
    },
    "pageConfig": {
      "showJump": true,
      "pcRow": 2,
      "pcColumn": 3,
      "moColumn": 1,
      "pageType": "hidden",
      "rolling": {
        "navigation": true,
        "pagenation": true,
        "autoScroll": true,
        "scrollTime": 4,
        "scrollType": "horizontal"
      },
      "marquee": {
        "navigation": true,
        "marqueeTime": 4
      },
      "loopItem": ".p_item"
    }
  },
  "i18n": {
    "zh_CN": {
      "pageWhole": "共",
      "pageItem": "条",
      "pageJump": "前往",
      "pageUnit": "页",
      "loadMore": "点击加载更多",
      "loadNow": "加载中",
      "noMore": "没有更多了",
      "noData": "暂无数据"
    }
  },
  "datasource": "prop",
  "dataFields": {
    "dataList": "dataList"
  },
  "scope": true,
  "container": true,
  "dataManage": true
}