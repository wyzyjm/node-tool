<div class="e_loop s_list" needjs="true">
    <div
        class="{{#eq prop.pageConfig.filterPosition 'left'}}position-left{{/eq}}{{#eq prop.pageConfig.filterPosition 'right'}}position-right{{/eq}}{{#eq prop.pageConfig.filterPosition 'top'}}position-top{{/eq}}{{#eq prop.pageConfig.filterPosition 'bottom'}}position-bottom{{/eq}}">

        {{#if datasource._config._dataFilter}}
        {{#if datasource._config._dataFilter.filter}}
        <div class="p_filter_wrapper">
            {{#datasource._config._dataFilter.fields}}
            <div class="p_kv_wrapper js-kv-wrapper" data-selectMode="{{selectMode}}">
                <div class="p_filter_key js-key">
                    {{name}}
                </div>
                {{!-- 标签式 --}}
                {{#eq displayMode 'tag'}}
                <div class="p_filter_value">
                    <div class="p_v_list js-value-list" data-index="{{@index}}">
                        {{#values}}
                        {{#eq filterType 'value'}}
                        <div class="p_v_item js-value-item " data-index="{{@index}}" data-valueType="{{valueType}}"
                            data-value="{{value}}">
                            {{#eq styleMode 'labelRadioButton'}}
                            <a href="javascript:;" class="js_editor_click p_fbutton">{{name}}</a>
                            {{/eq}}
                            {{#eq styleMode 'labelCheckboxButton'}}
                            <a href="javascript:;" class="js_editor_click p_mbutton">{{name}}</a>
                            {{/eq}}

                            {{#eq selectMode 'multiple'}}
                            {{#eq styleMode "labelCheckbox"}}
                            <label class="js_editor_click p_checkbox">
                                <input class="js_editor_click input-check p_input_check" type="checkbox" />{{name}}
                            </label>
                            {{/eq}}
                            {{/eq}}

                            {{#eq selectMode 'radio'}}
                            {{#eq styleMode "labelRadio"}}
                            <label class="js_editor_click p_radio">
                                <input class="js_editor_click input-radio p_input_radio" name="inputname{{../field}}" type="radio" />{{name}}
                            </label>
                            {{/eq}}
                            {{/eq}}

                        </div>
                        {{/eq}}
                        {{!-- 区间指定值 --}}
                        {{#eq filterType 'range'}}
                        <div class="p_v_item js-value-item" data-index="{{@index}}"
                            data-value="{{range.leftType}}&{{range.left}}&{{range.rightType}}&{{range.right}}">
                            {{#eq styleMode 'labelRadioButton'}}
                            <a href="javascript:;" class="js_editor_click p_fbutton">{{name}}</a>
                            {{/eq}}
                            {{#eq styleMode 'labelCheckboxButton'}}
                            <a href="javascript:;" class="js_editor_click p_mbutton">{{name}}</a>
                            {{/eq}}
                            {{#eq selectMode 'multiple'}}
                            {{#eq styleMode 'labelCheckbox'}}
                            <label class="p_mul_checkbox">
                                <input class="input-check p_input_check" type="checkbox" />{{name}}
                            </label>
                            {{/eq}}
                            {{/eq}}

                            {{#eq selectMode 'radio'}}
                            {{#eq styleMode 'labelRadio'}}
                            <label class="p_label_radio">
                                <input class="input-radio p_input_radio" name="inputname" type="radio" />{{name}}
                            </label>
                            {{/eq}}
                            {{/eq}}
                        </div>
                        {{/eq}}
                        {{/values}}
                    </div>
                </div>
                {{/eq}}
                {{#eq displayMode 'select'}}
                <div class="js-filter-select p_filter_select selectPickerWrapper">
                    <select class="hidden">

                    </select>
                </div>
                {{!-- <select class="form-control p_filter_select" data-index="{{@index}}">
                <option value="" data-index="-1">请选择</option>
                {{#values}}
                <option class="p_s_option" data-index="{{@index}}" value="{{value}}" data-valueType="{{valueType}}"
                    data-value="{{value}}">{{name}}</option>
                {{/values}}
            </select> --}}

                {{/eq}}

                {{#eq displayMode 'input'}}
                <div class="p_filter_value">
                    <div class="p_v_list js-value-list" data-index="{{@index}}">
                        {{!-- 普通 - slider --}}
                        {{#eq styleMode 'slider'}}
                        {{#values}}
                        <div class="p_v_item js-value-item " data-index="{{@index}}" data-valueType="{{valueType}}"
                            data-value="{{value}}">
                            <div class="input-slider p_input_slider"></div><span>{{name}}</span>
                        </div>
                        {{/values}}
                        {{/eq}}
                        {{!-- 普通 - 区间 --}}
                        {{#eq styleMode 'inputRange'}}
                        {{#values}}
                        <div class="p_v_item js-value-item " data-index="{{@index}}" data-valueType="{{valueType}}"
                            data-value="{{value}}">
                            <div class="js_normal p_normal">
                                <input class="form-control js_normal-start p_normal_start" type="text" value="" />
                                <span class="p_split">-</span>
                                <input class="form-control js_normal-end p_normal_end" type="text" />{{name}}
                            </div>
                        </div>
                        {{/values}}
                        {{/eq}}
                        {{!-- 普通 - 输入框 --}}
                        {{#eq styleMode 'input'}}
                        {{#values}}
                        <div class="p_v_item js-value-item " data-index="{{@index}}" data-valueType="{{valueType}}"
                            data-value="{{value}}">
                            <input class="js-input-match p_input_match" type="text" />{{name}}
                        </div>
                        {{/values}}
                        {{/eq}}
                        {{!-- 日期 - 日期 --}}
                        {{#eq styleMode 'date'}}
                        {{#values}}
                        <div class="p_v_item js-value-item " data-index="{{@index}}" data-valueType="{{valueType}}"
                            data-value="{{value}}">

                            <div class="j_date p_date_date">
                                <input class="form-control date-start  p_date_start" type="text" value="" />
                            </div>
                        </div>
                        {{/values}}
                        {{/eq}}
                        {{!-- 日期 - 日期周期 --}}
                        {{#eq styleMode 'dateRange'}}
                        {{#values}}
                        <div class="p_v_item js-value-item " data-index="{{@index}}" data-valueType="{{valueType}}"
                            data-value="{{value}}">

                            <div class="j_date p_date_range">
                                <input class="form-control date-start  p_date_start" type="text" value="" />
                                <span class="p_split">-</span>
                                <input class="form-control date-end  p_date_end" type="text" />
                                {{name}}
                            </div>
                        </div>
                        {{/values}}
                        {{/eq}}
                    </div>
                </div>
                {{/eq}}

            </div>
            {{/datasource._config._dataFilter.fields}}

            <div class="p_filter_result js-condition-wrapper">
                <span class="p_result_label">条件：</span>
            </div>
            
        </div>
        <br>
        {{/if}}
        {{/if}}
        
            {{#arrControl prop.dataList  prop.pageConfig.status}}
            <div class="p_list">
                {{#prop.dataList}}
                <div class="cbox-${id} p_loopitem"></div>
                {{/prop.dataList}}
            </div>
            <div class="p_page">
                {{{nPaging prop.page prop.pageConfig datasource i18nJson ${id}}}}
            </div>
            {{^}}
            {{!-- {{{nNoDataPrompt i18n.noData}}} --}}

            {{#prop.dataList_${id}}}
                {{#eq @index 0}}
                    <div class="cbox-${id}-1 p_nodata"></div>
                {{/eq}}
            {{/prop.dataList_${id}}}

            {{/arrControl}}
        
    </div>
    <input type="hidden" name="_config" value='{{$toJson datasource._config}}'>
    <input type="hidden" name="view" value='{{{view}}}'>
    {{!-- datasource page 数据总条数 --}}
    <input type="hidden" name="pageParamsJson" value='{{$toJson datasource.page}}'>
    <input type="hidden" name="i18nJson" value='{{i18nJson}}'>

</div>