  "tmpl": "<div class=\"${elemId} ${styleClass}\">\n    {{#${imgList}}}\n        {{#eq @index 0}}\n\t\t\t<div class=\"p_imgItem\" \n            style=\"width:calc(( 100% - 2 * {{../${space}}}px) / 3);height:100%\">\n                <!-- 链接 -->\n                {{#if href.value}}\t\n                <a href=\"{{$transferLink href compId}}\" target=\"{{href.target}}\">\n                {{/if}}\n                    <!-- 图片 -->\n                    {{{nLazyImage imageUrl title isLazy}}}\n                {{#if href.value}}\t\n                </a>\n                {{/if}}\n            </div>\n\t\t{{/eq}}\n        {{#eq @index 1}}\n\t\t\t<div class=\"p_imgItem\" \n            style=\"width:calc(( 100% - 2 * {{../${space}}}px) / 3);height:calc(( 100% - {{../${space}}}px) / 2)\">\n                <!-- 链接 -->\n                {{#if href.value}}\t\n                <a href=\"{{$transferLink href compId}}\" target=\"{{href.target}}\">\n                {{/if}}\n                    <!-- 图片 -->\n                    {{{nLazyImage imageUrl title isLazy}}}\n                {{#if href.value}}\t\n                </a>\n                {{/if}}\n            </div>\n\t\t{{/eq}}\n\t\t\n        {{#eq @index 2}}\n            <div class=\"p_imgItem\" \n            style=\"width:calc(( 100% - 2 * {{../${space}}}px) / 3);height:calc(( 100% - {{../${space}}}px) / 2)\">\n                <!-- 链接 -->\n                {{#if href.value}}\t\n                <a href=\"{{$transferLink href compId}}\" target=\"{{href.target}}\">\n                {{/if}}\n                    <!-- 图片 -->\n                    {{{nLazyImage imageUrl title isLazy}}}\n                {{#if href.value}}\t\n                </a>\n                {{/if}}\n            </div>\n\t\t{{/eq}}\n\n        {{#eq @index 3}}\n            <div class=\"p_imgItem\" \n            style=\"width:calc(( 100% - 2 * {{../${space}}}px) / 3);height:calc(( 100% - {{../${space}}}px) / 2)\">\n                <!-- 链接 -->\n                {{#if href.value}}\t\n                <a href=\"{{$transferLink href compId}}\" target=\"{{href.target}}\">\n                {{/if}}\n                    <!-- 图片 -->\n                    {{{nLazyImage imageUrl title isLazy}}}\n                {{#if href.value}}\t\n                </a>\n                {{/if}}\n            </div>\n\t\t{{/eq}}\n\n        {{#eq @index 4}}\n            <div class=\"p_imgItem\" \n            style=\"width:calc(( 100% - 2 * {{../${space}}}px) / 3);height:calc(( 100% - {{../${space}}}px) / 2)\">\n                <!-- 链接 -->\n                {{#if href.value}}\t\n                <a href=\"{{$transferLink href compId}}\" target=\"{{href.target}}\">\n                {{/if}}\n                    <!-- 图片 -->\n                    {{{nLazyImage imageUrl title isLazy}}}\n                {{#if href.value}}\t\n                </a>\n                {{/if}}\n            </div>\n\t\t{{/eq}}\n    {{/${imgList}}}\n</div>",
