  "tmpl": "<div class=\"${elemId} ${styleClass}\" needjs=\"true\">\r\n    {{#if ${bannerLists}}}\r\n    <div class=\"swiper-container\" >\r\n        <div class=\"swiper-wrapper p_swiperContainer\">\r\n            {{#each ${bannerLists}}}\r\n            <div class=\"swiper-slide p_slide\">\r\n                {{#eq type 1}}\r\n                    <div class=\"p_img\">\r\n                        <a href=\"{{#if href.value}}{{$transferLink href compId}}{{^}}javascript:;{{/if}}\" {{#if href.value}}target=\"{{href.target}}\"{{/if}}>\r\n                            {{#eq @index 0}}\r\n                            {{{nLazyImage imgUrl title 'false'}}}\r\n                            {{^}}\r\n                            {{{nLazyImage imgUrl title 'true'}}}\r\n                            {{/eq}}\r\n                        </a>\r\n                    </div>\r\n                {{/eq}}\r\n                {{#eq type 2}}\r\n                    <div class=\"p_video\">\r\n                        <div class=\"p_videoPoster\">\r\n                            {{{nLazyImage video.coverImgUrl title 'false'}}}\r\n                        </div>\r\n                        <div class=\"videoContent\">\r\n                        {{{lazyVideo video.url ../${settings}.videoMouted false}}}\r\n                        </div>\r\n                    </div>\r\n                {{/eq}}\r\n                <div class=\"p_info {{#eq type 2}}p_videoInfo{{/eq}}\">\r\n                    <div>\r\n                        {{#if ${settings}.showText}}\r\n                            {{#if title}}<h1 class=\"text-white p_btitle\">{{title}}</h1>{{/if}}\r\n                            {{#if subTitle}}<h6 class=\"text-white p_subtitle\">{{subTitle}}</h6>{{/if}}\r\n                            {{#if summary}}<p class=\"text-white p_summary\">{{summary}}</p>{{/if}}\r\n                        {{/if}}\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            {{/each}}\r\n        </div>\r\n        {{#if ${settings}.pagination}}\r\n        <div class=\"swiper-pagination p_pagenation\"></div>\r\n        {{/if}}\r\n        {{#if ${settings}.navigation}}\r\n        <div class=\"js_editor_click swiper-button-next p_btn_next\"></div>\r\n        <div class=\"js_editor_click swiper-button-prev p_btn_prev\"></div>\r\n        {{/if}}\r\n    </div>\r\n    {{^}}\r\n    {{{nNoDataPrompt ${i18n.noData}}}}\r\n    {{/if}}\r\n</div>",
