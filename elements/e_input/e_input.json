{
  "cname": "单行文本",
  "styleId": "e_input",
  "styleClass": "",
  "type": "form",
  "tmpl": "<div class=\"${elemId} ${styleClass} form-group {{#eq ${state} 'hidden'}}formHide{{/eq}}\" needjs=\"true\">\n    {{#if ${showLabel}}}\n    <label class=\"p_label\">\n        {{${label}}}\n        {{#if ${validate}.required.value}}<span class=\"text-danger p_required\">*</span>{{/if}}\n    </label>\n    {{/if}}\n    {{#eq ${state} 'readOnly'}}\n    <p class=\"p_formValue\">{{${value}}}</p>\n    {{/eq}}\n    <div class=\"{{#eq ${state} 'readOnly'}}hide{{/eq}}\">\n        <input type=\"text\" class=\"form-control p_input\" name=\"${elemId}\" \n            {{#if ${showPlaceholder}}}placeholder=\"{{${placeholder}}}\"{{/if}} \n            {{#if ${value}}}value=\"{{${value}}}\"{{/if}} \n            {{#eq ${state} 'disabled'}}disabled{{/eq}}\n        >\n        <div class=\"invalid-feedback\"></div>\n    </div>\n    <input type=\"hidden\" name=\"propJson\" value='{{{propJson}}}'>\n</div>",
  "css": [
    {
      "media": "",
      "cssText": ".e_input .p_number .p_input{\n    display: inline-block;\n    width: 80%;\n}"
    },
    {
      "media": "screen and (max-width:768px)",
      "cssText": ""
    },
    {
      "media": "screen and (min-width: 769px)",
      "cssText": ""
    },
    {
      "media": "screen and (max-width: 1024px) and (min-width: 769px)",
      "cssText": ""
    }
  ],
  "prop": {
    "showLabel": true,
    "label": "单行文本",
    "showPlaceholder": true,
    "placeholder": "请输入...",
    "type": "text",
    "value": "",
    "state": "normal",
    "validate": {
      "required": {
        "state": true,
        "value": true,
        "msg": "内容不能为空"
      },
      "minLength": {
        "state": true,
        "value": 5,
        "msg": "您输入的内容不能少于%len个字符"
      },
      "maxLength": {
        "state": true,
        "value": 128,
        "msg": "您输入的内容不能多于%len个字符"
      },
      "custom": {
        "state": false,
        "reg": "",
        "msg": ""
      }
    }
  },
  "i18n": {
    "zh_CN": {}
  },
  "datasource": "prop"
}