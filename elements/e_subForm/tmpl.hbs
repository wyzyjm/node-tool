<div class="e_subForm">
    {{#if prop.showLabel}}
    <div class="p_title">{{prop.label}}</div>
    {{/if}}
    <div class="p_thead">
        {{#prop.content}}
        <div class="p_th"></div>
        {{/prop.content}}
    </div>
    <div class="p_tbody">
        <div class="p_tr">
        {{#prop.content}}
        <div class="cbox-${id}-{{@index}} p_td"></div>
        {{/prop.content}}
        </div>
    </div>
    {{#if prop.showBtn}}
    <div>
        <button class="btn btn-primary btn-sm s_button1">{{prop.btnText}}</button>
    </div>
    {{/if}}
</div>