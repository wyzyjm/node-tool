<div class="e_register p_container" needjs="true">
    <h4 class="border-bottom p_title">欢迎注册</h4>
    <div class="p_wrapper">
        <div class="p_line" itemCode="confirmPassword" dataType="password" dataSubType="password" isRequire=true fixed="1">
            <label class="p_label"> <span class="text-danger p_required">*</span> 确认密码</label>
            <div class="form-control clearfix p_range">
                <input type="password" placeholder="请再次输入密码" class="p_input blurEvent getValue">
            </div>
            <div class="invalid-feedback"></div>
        </div>
        <div class="p_line codeLine" itemCode="code" isRequire='1'>
            <label class="p_label">验证码</label>
            <div class="form-control clearfix p_range">
                <input type="text" placeholder="短信验证码" class="p_codeInput blurEvent getValue">
                <button class="p_codeBtn p_time-1">获取验证码</button>
                <button class="p_codeBtn text-danger p_time-2" disabled></button>
            </div>
            <div class="invalid-feedback"></div>
        </div>
        <div class="p_mark">
            <div class="bg-white p_agreementWrapper">
                <h6>
                    用户注册协议
                    <svg t="1629190318737" class="icon closeAgreement" viewBox="0 0 1024 1024" version="1.1" xmlns="http://www.w3.org/2000/svg" p-id="1523" width="200" height="200"><path d="M787.8125 296.1875L296.28125 787.8125c-16.40625 16.40625-43.125 16.40625-59.625 0-16.40625-16.40625-16.40625-43.125 0-59.625l491.53125-491.53125c16.40625-16.40625 43.125-16.40625 59.625 0s16.5 43.125 0 59.53125z" p-id="1524" fill="#cdcdcd"></path><path d="M787.8125 787.8125c-16.40625 16.40625-43.125 16.40625-59.625 0L236.65625 296.1875c-16.40625-16.40625-16.40625-43.125 0-59.625s43.125-16.40625 59.625 0l491.53125 491.53125c16.5 16.59375 16.5 43.21875 0 59.71875z" p-id="1525" fill="#cdcdcd"></path></svg>
                </h6>
                <div class="p_text"> </div>
                <button class="bg-primary text-white p_agreementBtn">同意并继续</button>
            </div>
        </div>
        <div class="p_line" itemCode="agreement" isRequire='1'>
            <label class="p_label"></label>
            <div class="form-control clearfix p_range no_border">
                <p>
                    <input type="checkbox" class="getValue changeEvent"  />
                    <label>已阅读并同意<span class="text-danger openAgreement">《用户注册协议》</span></label>
                </p>
            </div>
            <div class="invalid-feedback"></div>
        </div>
        <div class="p_bLine">
            <button class="bg-primary text-white p_registerBtn">立即注册</button>
            <p class="p_loginBtn">已有账号？<span class="text-primary goLogin">请登录></span> </p>
        </div>
    </div>
</div>