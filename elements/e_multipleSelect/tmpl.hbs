<div class="e_multipleSelect s_form1 form-group {{#eq prop.state 'hidden'}}formHide{{/eq}}" needjs="true">
    {{#if prop.showLabel}}
    <label class="p_label">
        <span class="s_label">{{prop.label}}</span>
        {{#if prop.validate.required.value}}<span class="text-danger p_required">*</span>{{/if}}
    </label>
    {{/if}}
    {{#eq prop.state 'readOnly'}}
    <p class="s_input p_formValue"></p>
    {{/eq}}
    <div class="{{#eq prop.state 'readOnly'}}hide{{/eq}}">
        <div class="multSelect s_form-control s_input p_input"></div>
        <div class="invalid-feedback"></div>
        <input type="hidden" name="${elemId}">
    </div>
</div>
