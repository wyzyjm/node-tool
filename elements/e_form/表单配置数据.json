{
    "basic":{   //表单设置
        "formInfo":{   //表单信息
            "title":"留言表单",   //表单名称
            "name":"launchbook",   //表单英文名
            "app":""    //所属应用
        },
        "submitLimit":{   //表单提交限制
            "peopleLimit":1,   //表单提交人限制，1:所有人，2:会员
            "limitRull":1,   //限制规则，1：无限制，2：按ip限制，3：按会员限制
            "count":1,   //限制次数
            "interval":1,   //提交间隔
            "unit":1   //间隔单位，1：分钟，2：小时，3：天
        },
        "submitTime":{   //表单提交时间设置
            "limit":1,   //限制提交时间，1：限制，2：不限制
            "startTime":"2020-05-21 17:00",   //开始时间
            "endTime":"2021-05-21 17:00",   //结束时间
            "prompt":"对不起，提交表单时间不在允许提交范围内！"   //不在表单提交时间段提醒
        },
        "advanced":{
            "customState":[   //自定义表单处理状态
                {  
                    "label":"待审核",   //显示名称
                    "value":1,   //值
                    "select":true   //选中
                }
            ],
            "message":1   //1：开启留言回复，2：不开启留言回复
        }
    },
    "rule":[   //表单规则
        {
            "if":{//如果
                "item":"",   //字段项
                "equal":1,   //为，1：已填写，2：未填写
                "and":""   //且
            },
            "then":{
                "item":"",   //字段项
                "equal":1,   //1：显示，2：隐藏，3：选填，4：必填
            },
            "status":1   //1:正常，2：停用
        }
    ],
    "submit":{   //提交表单
        "type":1,   //提交业务类型，1：仅保存数据，2：保存数据并提交数据到页面，3：保存数据并链接到业务
        "obj1":{   //仅保存数据时
            "prompt":"",   //成功提示信息
            "formula":"",   //计算公式
            "afterSubmit":1,   //表单提交后，1：保存成功后，仅显示成功信息，2：保存成功后并跳转到链接，3：保存成功后，链接到指定文档
            "pageUrl":"",   //页面链接
            "fileUrl":""   //文档链接
        },
        "obj2":{   //保存数据并提交数据到页面时
            "pageUrl":"",   //页面链接
            "params":[   //传递参数
                {
                    "label":"服务编号",   //参数备注
                    "key":"id",   //参数名（英文）
                    "value":"$产品编号"   //表单项
                }
            ]
        },
        "obj3":{   //保存数据并链接到业务时
            "business":"",   //业务
            "params":[   //参数传递设置
                {
                    "label":"服务编号",   //参数备注
                    "key":"id",   //参数名（英文）
                    "value":"$产品编号"   //表单项
                }
            ]
        }
        
        
    },
    "async":{   //数据同步
        "status":1,   //是否开启数据同步，1：是，2：否
        "target":"",   //目标数据源
        "relation":[   //数据映射关系
            {
                "from":"",   //提交数据项
                "to":""   //目标数据项
            }
        ]
    },
    "notice":{   //表单通知
        "status":1,   //是否开启表单提交后通知，1：是，2：否
        "rules":[   //通知规则
            {
                "title":"事件名称",   //提醒时间名称
                "code":1,   //提醒节点，1：表单提交成功，2：表单审核通过，3：表单审核驳回，4：留言回复
                "user":1   //提醒对象，1：表单提交人，2：管理员
            }
        ]
    }
}






