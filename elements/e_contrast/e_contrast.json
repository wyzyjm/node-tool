      "cssText": ".e_contrast{\n\n}\n*{\n  box-sizing: border-box;\n}\n.p_compare{\n  background: #fff;\n  margin-left: 180px;\n}\n.p_left{\n  float: left;\n  width: 180px;\n  background:#f3f3f3\n}\n.p_right{\n  float: left;\n  width: calc(100% - 180px);\n  overflow-x: auto;\n}\n.p_rcontent{\n  min-width: 900px;\n}\n/*属性值行*/\n.p_row{\n  border-bottom: 1px solid #ccc;\n}\n.p_left .p_row{\n  border-right: 1px solid #ccc;\n  border-left: 1px solid #ccc;\n}\n/*第一行*/\n.p_rowt{\n  border-top: 1px solid #ccc;\n  width: 100%;\n}\n/*参数组标题行*/\n.p_rowgt{\n  background: #f3f3f3;\n  padding: 0;\n  border-bottom: 1px solid #ccc;\n}\n.p_left .p_rowgt{\n  border-right: none;\n}\n/*参数单元格*/\n.p_ceil{\n  padding: 10px 10px;\n  text-align: center;\n  position: relative;\n  height: 110px;\n  overflow: hidden;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n.p_right .p_ceil{\n  float: left;\n  width: 25%;\n  border-right: 1px solid #ccc;\n}\n.p_right .p_row{\n  background: #fff;\n}\n.p_row.light{\n  background:#e1f6ff\n}\n.p_rowgt .p_ceil{\n  float: none;\n  width: 100%;\n  padding: 10px;\n  line-height: 40px;\n  height: 60px;\n  background: #f3f3f3;\n}\n.p_rowt .p_ceil{\n  height: 135px;\n  padding:10px 10px;\n}\n\n.p_product{\n  align-items:flex-start;\n  justify-content: flex-start;\n}\n.p_product_info{\n  padding: 0 10px 0 100px;\n  line-height: 26px;\n  text-align: left;\n}\n.p_product_title{\n  max-height: 52px;\n  overflow: hidden;\n  text-align: justify;\n}\n.p_product_img{\n  width: 90px;\n  height: 90px;\n  position: absolute;\n  left: 10px;\n  top: 15px;\n  background: #f9f9f9;\n}\n.p_product_img img{\n  display: block;\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n}\n.p_arrow{\n  display: inline-block;\n  vertical-align: middle;\n  margin-right: 10px;\n  width: 16px;\n  height: 16px;\n  border: 2px solid #999;\n  border-radius: 2px;\n  position: relative;\n}\n.p_arrow::after{\n  position: absolute;\n  width: 13px;\n  height: 12px;\n  left: 0;\n  top: 0;\n  line-height: 12px;\n  color: #666;\n  content: \"-\";\n}\n.p_arrow.zd::after{\n  content: \"+\";\n}\n.fixedtop{\n  padding-top: 140px;\n}\n.fixedtop .fixeddiv,.fixedtop .p_lop{\n  overflow-x: auto;\n  z-index: 9;\n  position: fixed;\n  top: 0;\n  width: inherit;\n  background:inherit;\n  box-shadow: 5px 5px 5px #ccc;\n\n}\n\n.p_menu{\n  position: fixed;\n  left: 0;\n  top: 30px;\n  width:180px;\n  z-index: 9;\n}\n.p_pcm{\n  padding:30px 40px 30px 10px;\n  position: relative;\n}\n.p_pcm::after{\n  content: \" \";\n  position: absolute;\n  height: 100%;\n  border-right: 1px solid #666;\n  right: 20px;\n  top: 0;\n  z-index: 4;\n}\n.p_pcl{\n  line-height: 20px;\n  padding: 10px 0;\n  text-align: right;\n  position: relative;\n}\n.p_pcl::after{\n  content: \"\";\n  position: absolute;\n  width: 8px;\n  height: 8px;\n  border-radius: 4px;\n  background: #666;\n  right: -23px;\n  top: 16px;\n  z-index: 5;\n}\n.p_pcl.active{\n  color: #007bff;\n}\n.p_pcl.active::after{\n  background: #007bff;\n}\n.p_mmenu{\n  display: none;\n}\n\n\n.hideSame {\n  display: none;\n}\n.hideSame + label {\n  display: inline-block;\n  width: 40px;\n  height: 20px;\n  position: relative;\n  -webkit-transition: 0.3s;\n  transition: 0.3s;\n  margin: 0px 20px;\n  box-sizing: border-box;\n}\n.hideSame + label:after {\n  content: '';\n  display: block;\n  position: absolute;\n  left: 0px;\n  top: 0px;\n  width: 20px;\n  height: 20px;\n  -webkit-transition: 0.3s;\n  transition: 0.3s;\n  cursor: pointer;\n}\n.hideSame:checked + label {\n  background: #52b1ff;    \n}\n.hideSame:checked + label:after {\n  background: #fff;\n}\n.hideSame:checked + label:after {\n  left: calc(100% - 20px);\n}\n.hideSame + label {\n  background: #ddd;\n  border-radius: 20px;\n  box-shadow: 1px 1px 3px #aaa;\n}\n.hideSame + label:after {\n  background: #fff;\n  border-radius: 50%;\n  box-shadow: 1px 1px 3px #aaa;\n}"
