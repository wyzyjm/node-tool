      "cssText": ".e_relevantItem{\n    min-height: 20px;\n    width: 100%;\n}\n.e_relevantItem .p_thead{\n    height: 36px;\n    line-height: 36px;\n    display: flex;\n    background-color: #f0f0f0;\n}\n.e_relevantItem .p_tbody{\n    min-height: 36px;\n    margin-bottom: 20px;\n}\n.e_relevantItem .p_tr{\n    display: flex;\n}\n.e_relevantItem .p_th,.p_td{\n    flex: 1;\n}\n.e_relevantItem .p_screen{\n    margin-bottom: 20px;\n}\n.e_relevantItem .p_modelDialog{\n    max-width: 960px;\n}\n.e_relevantItem .p_modalContent{\n    border-radius: 6px;\n}\n.e_relevantItem .p_category{\n    display: inline-block;\n    width: 200px;\n}\n.e_relevantItem .p_search{\n    float: right;\n    width: 260px;\n}\n.e_relevantItem .p_listImg{\n    width: 30px;\n    height: 30px;\n}\n.e_relevantItem .p_listImg img{\n    width: 100%;\n    height: 100%;\n    object-fit: contain;\n}\n.e_relevantItem .p_listWord{\n    line-height: 30px;\n    max-height: 60px;\n    overflow: hidden;\n}\n.e_relevantItem .p_tabel{\n    border: 1px  solid #f0f0f0;\n}\n.e_relevantItem .p_tabel thead td{\n    border-bottom: none;\n    background-color: #e0e0e0;\n    padding:15px 10px;\n}\n.e_relevantItem .p_tabelList thead td{\n    border-bottom: none;\n    background-color: #f0f0f0;\n    height: 36px;\n    overflow: hidden;\n    padding:0 10px;\n    vertical-align: middle;\n}\n.e_relevantItem .p_tabel td, \n.e_relevantItem .p_tabel th{\n    padding: 5px 10px;\n    border: 1px  solid #f0f0f0;\n}\n.e_relevantItem .p_tabelList td{\n    padding: 5px 10px;\n    border:0;\n}\n.e_relevantItem .p_listSelect{\n    width: 50px;\n    text-align: center;\n}\n.e_relevantItem .p_listSelect input{\n    vertical-align: middle;\n    margin-top: 3px;\n}\n.e_relevantItem .p_listPage{\n    float: right;\n}\n.e_relevantItem .p_selectAll{\n    padding-left: 25px;\n}\n.e_relevantItem .p_remove_fixed{\n    width: 40px;\n    display: block;\n    color: #007bff;\n}"
