{
  "cname": "多选项",
  "styleId": "e_checkbox",
  "styleClass": "s_form1",
  "type": "form",
  "putSubForm": true,
  "tmpl": "<fieldset class=\"${elemId} ${styleClass} form-group {{#eq ${state} 'hidden'}}formHide{{/eq}}\" needjs=\"true\" {{#if ${validate}.required.value}}required{{/if}}>\n    {{#if ${showLabel}}}\n    <legend class=\"col-form-label p_label\">\n        <span class=\"s_label\">{{${label}}}</span>\n    </legend>\n    {{/if}}\n    {{#eq ${state} 'readOnly'}}\n    <p class=\"p_formValue s_input\"></p>\n    {{/eq}}\n    <div class=\"{{#eq ${state} 'readOnly'}}hide{{/eq}}\">\n        <div class=\"p_input\">\n            {{#${options}}}\n            <div class=\"form-check form-check-inline p_radioItem\">\n                <input class=\"form-check-input s_form-control\" type=\"checkbox\" name=\"${elemId}\" id=\"${elemId}-{{@index}}\" value=\"{{value}}\" {{#if select}}checked{{/if}} {{#eq ../${state} 'disabled'}}disabled{{/eq}}>\n                <label class=\"form-check-label s_other\" for=\"${elemId}-{{@index}}\">{{label}}</label>\n            </div>\n            {{/${options}}}\n        </div>\n        <div class=\"invalid-feedback\"></div>\n    </div>\n</fieldset>",
  "css": [
    {
      "media": "",
      "cssText": ""
    },
    {
      "media": "screen and (max-width:768px)",
      "cssText": ""
    },
    {
      "media": "screen and (min-width: 769px)",
      "cssText": ""
    },
    {
      "media": "screen and (max-width: 1024px) and (min-width: 769px)",
      "cssText": ""
    }
  ],
  "prop": {
    "showLabel": true,
    "label": "多选项",
    "state": "normal",
    "options": [
      {
        "label": "读书",
        "value": "1"
      },
      {
        "select": true,
        "label": "旅游",
        "value": "2"
      },
      {
        "label": "音乐",
        "value": "3"
      },
      {
        "label": "篮球",
        "value": "4"
      }
    ],
    "datasourceList": [],
    "ddItemList": [],
    "validate": {
      "required": {
        "state": true,
        "value": true,
        "msg": "选项不能为空"
      }
    }
  },
  "i18n": {
    "zh_CN": {}
  },
  "datasource": "prop",
  "dataFields": {
    "label": "label",
    "name": "name",
    "options": "options",
    "options$label": "label",
    "options$value": "value",
    "options1": "options1",
    "options1$label": "label",
    "options1$value": "value",
    "showLabel": "showLabel",
    "state": "state",
    "validate": "validate",
    "datasourceList": "datasourceList",
    "ddItemList": "ddItemList"
  },
  "scope": true
}