  "tmpl": "<div class=\"${elemId} ${styleClass} form-group\" needjs=\"true\" required>\n    {{#if ${showLabel}}}\n    <label class=\"p_label\">\n        <span class=\"s_label\">{{${label}}}</span>\n    </label>\n    {{/if}}\n    <div>\n        <div class=\"form-check form-check-inline p_input\">\n            <input class=\"form-check-input s_form-control\" type=\"checkbox\" name=\"${elemId}\" id=\"${elemId}-{{@index}}\" {{#if ${validate}.select.value}}checked{{/if}}>\n            <label class=\"form-check-label s_other\" for=\"${elemId}-{{@index}}\">{{${placeholder}}}</label>\n            <a href=\"javascript:;\" class=\"text-primary p_showAgreement\" data-toggle=\"modal\" data-target=\"#${elemId}-relevantModal\">{{${linkText}}}</a>\n        </div>\n        <div class=\"invalid-feedback\"></div>\n    </div>\n\n    <div class=\"modal fade\" id=\"${elemId}-relevantModal\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"modalTitle\" aria-hidden=\"true\">\n        <div class=\"modal-dialog modal-dialog-centered modal-lg\" role=\"document\">\n            <div class=\"modal-content p_modalContent\">\n                <div class=\"modal-header\">\n                    <h6 class=\"modal-title\" id=\"modalTitle\">{{${i18n.title}}}</h6>\n                    <button type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-label=\"Close\"><span class=\"text-white\" aria-hidden=\"true\">&times;</span></button>\n                </div>\n                <div class=\"modal-body p_agreeContent\">\n                    {{{${content}}}}                  \n                </div>\n                <div class=\"modal-footer p_agreeBtn\">\n                    <button type=\"button\" class=\"btn btn-primary btn-sm p_confirm\" data-dismiss=\"modal\">{{${i18n.aggree}}}</button>\n                </div>\n            </div>\n        </div>\n    </div>\n</div>",
