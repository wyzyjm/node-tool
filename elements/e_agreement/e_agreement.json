{
  "cname": "条款条件",
  "styleId": "e_agreement",
  "styleClass": "s_form1",
  "tmpl": "<div class=\"${elemId} ${styleClass} form-group\">\n    {{#if ${showLabel}}}\n    <label class=\"p_label\">\n        <span class=\"s_label\">{{${label}}}</span>\n        <span class=\"text-danger p_required\">*</span>\n    </label>\n    {{/if}}\n    <div>\n        <div class=\"form-check form-check-inline p_input\">\n            <input class=\"form-check-input s_form-control\" type=\"checkbox\" name=\"${elemId}\" id=\"${elemId}-{{@index}}\" {{#if ${validate}.select.value}}checked{{/if}}>\n            <label class=\"form-check-label s_other\" for=\"${elemId}-{{@index}}\">{{${placeholder}}}</label>\n            <a href=\"javascript:;\" class=\"text-primary p_showAgreement\">{{${linkText}}}</a>\n        </div>\n        <div class=\"invalid-feedback\"></div>\n    </div>\n</div>",
  "css": [
    {
      "media": "",
      "cssText": ".e_agreement .p_showAgreement{\n    margin-left: 10px;\n}"
    },
    {
      "media": "screen and (max-width:768px)",
      "cssText": ""
    },
    {
      "media": "screen and (min-width: 769px)",
      "cssText": ""
    },
    {
      "media": "screen and (max-width: 1024px) and (min-width: 769px)",
      "cssText": ""
    }
  ],
  "prop": {
    "showLabel": true,
    "label": "条款协议",
    "placeholder": "我已阅读并接受条款及条件",
    "linkText": "查看使用条款",
    "content": "<p>这里是条款内容，字数不限</p>",
    "validate": {
      "select": {
        "value": true
      }
    }
  },
  "i18n": {
    "zh_CN": {}
  },
  "datasource": "prop",
  "type": "form"
}