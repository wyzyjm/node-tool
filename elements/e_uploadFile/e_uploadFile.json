{
  "cname": "文件型",
  "styleId": "e_uploadFile",
  "styleClass": "",
  "type": "form",
  "tmpl": "<div class=\"${elemId} ${styleClass}\">\n    {{#if ${showLabel}}}\n    <label class=\"p_label\">\n        {{${label}}}\n        {{#if ${validate}.required.value}}<span class=\"text-danger p_required\">*</span>{{/if}}\n    </label>\n    {{/if}}\n    <div class=\"p_fileList clearfix\">\n        <div class=\"p_fileItem\">\n            <span class=\"text-primary\">xxxxx.mp4</span>\n            <span class=\"p_op\">\n                <span class=\"text-warning p_size\">458.3MB</span>\n                <span class=\"text-danger p_del\">\n                    <svg t=\"1628068788639\" class=\"icon\" viewBox=\"0 0 1024 1024\" version=\"1.1\" xmlns=\"http://www.w3.org/2000/svg\" p-id=\"8175\" width=\"100\" height=\"100\"><path d=\"M746.23228413 310.58739301h-52.07424984V209.87430018H329.8419657v100.71309284H225.75457011v50.34975706H277.76771587v402.83200332c0 27.83630043 20.9722761 50.35654641 46.84645683 50.35654642h374.7716546c25.87418073 0 46.84645683-22.54740335 46.84645683-50.35654641V360.93715008h52.04709247v-50.34975706zM433.92257196 713.41939633h-52.03351379v-302.12569982H433.92257196z m104.08739559 0h-52.03351379v-302.12569982h52.04030313z m104.08739559 0H590.07742804v-302.12569982h52.04709248z m0-402.83200331H381.88905817v-50.34975708h260.22188366z\" p-id=\"8176\"></path></svg>\n                </span>\n            </span>\n        </div>\n    </div>\n    <div class=\"p_upload\">\n        <button class=\"e_formBtn btn btn-primary btn-sm s_button1 p_uploadBtn\">上传</button>\n        <span>{{${placeholder}}}</span>\n    </div>\n    <div class=\"invalid-feedback\"></div>\n    <input type=\"hidden\" name=\"propJson\" value='{{{propJson}}}'>\n</div>",
  "css": [
    {
      "media": "",
      "cssText": ".e_uploadFile{\n\n}\n.e_uploadFile .p_upload{\n    line-height: 30px;\n    background-color: #f0f0f0;\n}\n.e_uploadFile .p_uploadBtn{\n    width: 80px;\n    margin-right: 20px;\n}\n.e_uploadFile .p_fileItem{\n    margin: 10px 0 20px;\n    overflow: hidden;\n}\n.e_uploadFile .p_op{\n    float: right;\n}\n.e_uploadFile .p_size{\n    margin-right: 10px;\n}\n.e_uploadFile .p_del{\n    cursor: pointer;\n}"
    },
    {
      "media": "screen and (max-width:768px)",
      "cssText": ""
    },
    {
      "media": "screen and (min-width: 769px)",
      "cssText": ""
    },
    {
      "media": "screen and (max-width: 1024px) and (min-width: 769px)",
      "cssText": ""
    }
  ],
  "prop": {
    "showLabel": true,
    "label": "文件型",
    "showPlaceholder": true,
    "placeholder": "请上传2M以内的ISO格式图片",
    "limit": 1,
    "size": 5,
    "unit": "kb",
    "accept": [
      ".iso",
      ".rar",
      ".zip"
    ],
    "validate": {
      "required": {
        "state": true,
        "value": true,
        "msg": "内容不能为空"
      }
    }
  },
  "i18n": {
    "zh_CN": {}
  },
  "datasource": "prop"
}