{
  "cname": "标签型",
  "styleId": "e_label",
  "styleClass": "s_form1",
  "type": "form",
  "tmpl": "<fieldset class=\"${elemId} ${styleClass} form-group\" needjs=\"true\" {{#if ${validate}.required.value}}required{{/if}}>\n    {{#if ${showLabel}}}\n    <legend class=\"col-form-label p_label\">\n        <span class=\"s_label\">{{${label}}}</span>\n    </legend>\n    {{/if}}\n    <div class=\"p_input\">\n        {{#${options}}}\n        <div class=\"form-check form-check-inline p_labelItem\">\n            <input class=\"form-check-input s_form-control\" type=\"checkbox\" name=\"${elemId}\" id=\"${elemId}-{{@index}}\" value=\"{{value}}\" {{#if select}}checked{{/if}}>\n            <label class=\"form-check-label s_other\" for=\"${elemId}-{{@index}}\">{{label}}</label>\n        </div>\n        {{/${options}}}\n    </div>\n    <div class=\"invalid-feedback\"></div>\n</fieldset>",
