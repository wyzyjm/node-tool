{
  "cname": "标签型",
  "styleId": "e_label",
  "styleClass": "",
  "type": "form",
  "tmpl": "<fieldset class=\"${elemId} ${styleClass} form-group\" needjs=\"true\">\n    {{#if ${showLabel}}}\n    <legend class=\"col-form-label p_label\">\n        {{${label}}}\n        {{#if ${validate}.required.value}}<span class=\"text-danger p_required\">*</span>{{/if}}\n    </legend>\n    {{/if}}\n    <div class=\"p_input\">\n        {{#${options}}}\n        <div class=\"form-check form-check-inline p_labelItem\">\n            <input class=\"form-check-input\" type=\"checkbox\" name=\"${elemId}\" id=\"${elemId}-{{@index}}\" value=\"{{value}}\" {{#if select}}checked{{/if}}>\n            <label class=\"form-check-label\" for=\"${elemId}-{{@index}}\">{{label}}</label>\n        </div>\n        {{/${options}}}\n    </div>\n    <div class=\"invalid-feedback\"></div>\n    <input type=\"hidden\" name=\"propJson\" value='{{{propJson}}}'>\n</fieldset>",
  "css": [
    {
      "media": "",
      "cssText": ".e_label .p_required{\n    vertical-align: middle;\n    font-size: 1.4em;\n    line-height: 100%;\n    position: relative;\n    font-weight: bold;\n    top: 0.15em;\n    padding-left: 5px;\n}\n.e_label .p_labelItem{\n    margin-right: 10px;\n}\n.e_label .p_labelItem input{\n    display: none;\n}\n.e_label .p_labelItem label{\n    padding: 0 10px;\n    border-radius: 4px;\n}\n.e_label .p_labelItem input:checked+label{\n    background-color: #ed6f2d;\n    color: #fff;\n}"
    },
    {
      "media": "screen and (max-width:768px)",
      "cssText": ""
    },
    {
      "media": "screen and (min-width: 769px)",
      "cssText": ""
    },
    {
      "media": "screen and (max-width: 1024px) and (min-width: 769px)",
      "cssText": ""
    }
  ],
  "prop": {
    "showLabel": true,
    "label": "标签型",
    "limit": 1,
    "options": [
      {
        "label": "选项1",
        "value": "1"
      },
      {
        "select": true,
        "label": "选项2",
        "value": "2"
      },
      {
        "label": "选项3",
        "value": "3"
      },
      {
        "label": "选项4",
        "value": "4"
      },
      {
        "label": "选项5",
        "value": "5"
      },
      {
        "label": "选项6",
        "value": "6"
      }
    ],
    "validate": {
      "required": {
        "state": true,
        "value": true,
        "msg": "内容不能为空"
      }
    }
  },
  "i18n": {
    "zh_CN": {}
  },
  "datasource": "prop"
}