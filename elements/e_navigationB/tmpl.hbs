<div class="e_navigationB" needjs="true">
    {{#if prop.dataList}}
    <p class="p_iconBox">
        {{{prop.menu}}}
        {{{prop.close}}}
    </p>
    <ul class="bg-white p_navBox1">
        {{#each prop.dataList}}
        <li class="border-bottom p_navItem1">
            <p>
                <a href="{{#if href}}{{href}}{{^}}javascript:;{{/if}}" target="{{#eq target 'BLANk'}}_blank{{/eq}}">
                    <span>{{name}}</span>
                    {{#if children}}{{{../prop.arrow}}}{{/if}}
                </a>
            </p>
            {{#if children}}
            <ul class="bg-white border p_navBox2">
                {{#each children}}
                <li class="p_navItem2">
                    <p>
                        <a href="{{#if href}}{{href}}{{^}}javascript:;{{/if}}" target="{{#eq target 'BLANk'}}_blank{{/eq}}">
                            {{#if imgUrl}}
                            <span class="p_img">{{{nLazyImage imgUrl name 'true'}}}</span>
                            {{/if}}
                            <span>{{name}}</span>
                        </a>
                    </p>
                </li>
                {{/each}}
            </ul>
            {{/if}}
        </li>
        {{/each}}
    </ul>
    {{^}}
    {{noDataPrompt}}
    {{/if}}
</div>