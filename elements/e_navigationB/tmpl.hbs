<div class="e_navigationB" needjs="true">
    {{#if prop.dataList}}
    <div class="p_navButton">
        <p class="p_iconBox">
            {{{prop.menu}}}
        </p>
    </div>
    <div class="p_navContent">
        <div class="p_navButton">
            <p class="p_iconBox">
                {{{prop.close}}}
            </p>
        </div>
        <ul class="p_navBox1">
            {{#each prop.dataList}}
            <li class="border-bottom p_navItem1">
                <p class="p_menu1Item js_editor_click">
                    <a href="{{#if url}}{{url}}{{^}}javascript:;{{/if}}" {{#if url}}target="{{#eq target 'blank'}}_blank{{/eq}}"{{/if}}>
                        <span>{{navName}}</span>
                    </a>
                    {{#if children}}{{{../prop.arrow}}}{{/if}}
                </p>
                {{#if this.children}}
                <ul class="p_navBox2">
                    {{#each this.children}}
                    <li class="p_navItem2">
                        <p class="p_menu2Item">
                            <a href="{{#if url}}{{url}}{{^}}javascript:;{{/if}}" {{#if url}}target="{{#eq target 'blank'}}_blank{{/eq}}"{{/if}}>
                                {{#if img}}
                                <span class="p_img">{{{nLazyImage img navName 'true'}}}</span>
                                {{/if}}
                                <span>{{navName}}</span>
                            </a>
                        </p>
                    </li>
                    {{/each}}
                </ul>
                {{/if}}
            </li>
            {{/each}}
        </ul>
    </div>
    {{^}}
    {{{nNoDataPrompt i18n.noData}}}
    {{/if}}
</div>