{
  "cname": "预览商品",
  "styleId": "e_preView",
  "styleClass": "",
  "tmpl": "<div class=\"${elemId} ${styleClass}\" needjs=\"true\">\r\n    {{#${list}}}\r\n        <div class=\"p_img_div\" >\r\n            <a href=\"{{${list$href}}}\" target=\"{{${list$target}}}\" class=\"p_img_a\" bind=\"href-id\" >\r\n                <img src=\"{{${list$coverImage}}}\" alt=\"\" class=\"p_img\" bind=\"src-coverImage\" >\r\n            </a>\r\n        </div>\r\n        {{#if ../${showImages} }}\r\n        <div class=\"p_scroll\"    panel=\"checkbox-showImages\">\r\n            <ul class=\"p_scroll_main\" bind=\"arr-images\" >\r\n                {{#images}}\r\n               \r\n                {{#eq  @index  0}}\r\n                <li class=\"p_scroll_item curr\" >\r\n                    <a href=\"JavaScript:;\" class=\"p_scroll_item_a \" >\r\n                        <img src=\"{{${list$url}}}\" alt=\"{{${list$title}}}\" class=\"p_scroll_item_img\" bind=\"src-url|alt-title\" >\r\n                    </a>\r\n                </li>\r\n                {{/eq}}\r\n                {{#gt @index  0}}\r\n                <li class=\"p_scroll_item\" >\r\n                    <a href=\"JavaScript:;\" class=\"p_scroll_item_a\" >\r\n                        <img src=\"{{${gt @index  0$url}}}\" alt=\"{{${gt @index  0$title}}}\" class=\"p_scroll_item_img\" bind=\"src-url|alt-title\" >\r\n                    </a>\r\n                </li>\r\n                {{/gt}}\r\n                {{/images}}\r\n        </div>\r\n        {{/if}}\r\n    {{/${list}}}\r\n    \r\n</div>",
  "css": [
    {
      "media": "",
      "cssText": "\n.e_preView .p_scroll_main{\n    display: flex;\n    flex-wrap: wrap;\n}\n.e_preView .p_scroll_item{\n    width: 20%;\n   \n}\n.e_preView .p_scroll_item_img{\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n}\n.e_preView .p_img{\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n}\n.e_preView .p_img_div{\n    margin-bottom: 8px;\n}\n.e_preView .p_img_a{\n    display: block;\n}\n.e_preView .p_scroll_item_a{\n    display: block;\n  border: 1px solid #ddd;\n  padding: 1px;\n  cursor: default;\n  margin-right: 8px;\n}\n.e_preView .p_scroll_item.curr a{\n    border-color:rgba(64, 158, 255, 1);\n}"
    },
    {
      "media": "screen and (max-width:768px)",
      "cssText": ""
    },
