      "cssText": "\r\n.e_preView .p_scroll_main{\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n    height: 44px;\r\n}\r\n.e_preView .p_scroll_item{\r\n    width: 20%;\r\n   \r\n}\r\n.e_preView .p_scroll_item_img{\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n}\r\n.e_preView .p_img{\r\n    width: 100%;\r\n    height: 100%;\r\n    object-fit: cover;\r\n}\r\n.e_preView .p_img_div{\r\n    margin-bottom: 8px;\r\n}\r\n.e_preView .p_img_a{\r\n    height: 240px;\r\n    display: block;\r\n}\r\n.e_preView .p_img_a img{\r\n    width: 100%;\r\n    height: 100%;\r\n    object-fit: cover;\r\n}\r\n.e_preView .p_scroll_item_a{\r\n    display: block;\r\n  border: 1px solid #ddd;\r\n  height: 42px;\r\n  padding: 1px;\r\n  cursor: default;\r\n  margin-right: 8px;\r\n}\r\n.e_preView .p_scroll_item.curr a{\r\n    border-color:rgba(64, 158, 255, 1);\r\n}"
