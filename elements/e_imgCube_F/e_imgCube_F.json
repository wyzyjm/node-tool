  "tmpl": "<div class=\"${elemId} ${styleClass}\">\r\n    {{#${imgList}}}\r\n\t\t<!-- 前3个 -->\r\n\t\t{{#lt @index 3}}\r\n\t\t\t<div class=\"p_imgItem \" \r\n            style=\"width:calc(( 100% - 2 * {{../${space}}}px) / 3);height:calc(( 100% - {{../${space}}}px) / 2)\">\r\n                <!-- 链接 -->\r\n                {{#if href.value}}\t\r\n                <a href=\"{{$transferLink href compId}}\" target=\"{{href.target}}\">\r\n                {{/if}}\r\n                    <!-- 图片 -->\r\n                    {{{nLazyImage imageUrl title \"{{isLazy}}\"}}}\r\n                {{#if href.value}}\t\r\n                </a>\r\n                {{/if}}\r\n            </div>\r\n\t\t{{/lt}}\r\n\r\n        {{#eq @index 3}}\r\n            <div class=\"p_imgItem \" \r\n            style=\"width:calc(( 100% - {{../${space}}}px) / 2);height:calc(( 100% - {{../${space}}}px) / 2)\">\r\n                <!-- 链接 -->\r\n                {{#if href.value}}\t\r\n                <a href=\"{{$transferLink href compId}}\" target=\"{{href.target}}\">\r\n                {{/if}}\r\n                    <!-- 图片 -->\r\n                    {{{nLazyImage imageUrl title \"{{isLazy}}\"}}}\r\n                {{#if href.value}}\t\r\n                </a>\r\n                {{/if}}\r\n            </div>\r\n\t\t{{/eq}}\r\n         {{#eq @index 4}}\r\n            <div class=\"p_imgItem \" \r\n            style=\"width:calc(( 100% - {{../${space}}}px) / 2);height:calc(( 100% - {{../${space}}}px) / 2)\">\r\n                <!-- 链接 -->\r\n                {{#if href.value}}\t\r\n                <a href=\"{{$transferLink href compId}}\" target=\"{{href.target}}\">\r\n                {{/if}}\r\n                    <!-- 图片 -->\r\n                    {{{nLazyImage imageUrl title \"{{isLazy}}\"}}}\r\n                {{#if href.value}}\t\r\n                </a>\r\n                {{/if}}\r\n            </div>\r\n\t\t{{/eq}}\r\n    {{/${imgList}}}\r\n</div>\r\n",
