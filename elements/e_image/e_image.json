{
  "cname": "图片",
  "styleId": "e_image",
  "styleClass": "s_img",
  "tmpl": "<div class=\"${elemId} ${styleClass}\">\r\n    {{#each ${imgList}}}\r\n        {{#eq @index 0}}\r\n        {{#if ${href}.value}}\r\n            <a href=\"{{$transferLink ${href} compId}}\" target=\"{{${href}.target}}\">\r\n        {{/if}}\r\n        {{{nLazyImage imageUrl ${title} ${setting}.isLazy}}}\r\n        {{#if ${href}.value}}\r\n            </a>\r\n        {{/if}}\r\n        {{/eq}}\r\n    {{/each}}\r\n</div>",
  "css": [
    {
      "media": "",
      "cssText": ".e_image{\r\n    overflow: hidden;\r\n}\r\n.e_image img{\r\n    width: 100%;\r\n    height: 100%;\r\n    object-fit: contain;\r\n}\r\n"
    },
    {
      "media": "screen and (max-width:768px)",
      "cssText": ""
    },
    {
      "media": "screen and (min-width: 769px)",
      "cssText": ""
    },
    {
      "media": "screen and (max-width: 1024px) and (min-width: 769px)",
      "cssText": ""
    }
  ],
  "prop": {
    "imgList": [
      {
