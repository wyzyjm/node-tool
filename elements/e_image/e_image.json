      }
    ],
    "title": "图片名称",
    "href": {
      "type": "",
      "value": "",
      "target": ""
    },
    "setting": {
      "isLazy": "true",
      "fit": "contain"
    },
    "space": 0
  },
  "i18n": {
    "zh_CN": {}
  },
