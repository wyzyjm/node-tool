{
  "cname": "分类树-横向",
  "styleId": "e_categoryC",
  "styleClass": "",
  "tmpl": "<div class=\"${elemId} ${styleClass}\" needjs='true'>\n    <div class=\"p_c_tree\">\n        {{#if ${cate}}}\n            {{#tree ${cate}}}\n                {{#if @nodeBegin}}<ul class=\"p_c_content\">{{/if}}\n                <li class=\"p_c_item\">\n                    <span class=\"p_c_title\">\n                        <a href=\"{{#if categoryUrl}}{{categoryUrl}}{{^}}javascript:;{{/if}}\" class=\"btn btn-primary p_c_title {{#eq selected true}}active{{/eq}}\" target=\"{{#eq target 'BLANK'}}_blank{{/eq}}\">\n                            <span>{{categoryName}}</span>\n                        </a>\n                    </span>\n                    {{tree children}}\n                </li>\n                {{#if @nodeEnd}}</ul>{{/if}}\n            {{/tree}}\n        {{^}}\n            {{nNoDataPrompt ${i18n.noData}}}\n        {{/if}}\n    </div>\n</div>",
  "css": [
    {
      "media": "",
      "cssText": ".e_categoryC{\n    user-select:none;\n}\n.e_categoryC .p_c_tree>ul>li{\n    display: inline-block;\n}\n.e_categoryC .p_c_item{\n    position:relative;\n    width: 140px;\n    margin-bottom:1px;\n}\n.e_categoryC .p_c_title{\n    font-size: inherit;\n    width: 100%;\n    overflow: hidden;\n    text-overflow: ellipsis;\n    white-space: nowrap;\n}\n.e_categoryC .p_c_item>.p_c_content{\n    position: absolute;\n    left: 100%;\n    top: 0;\n    display: none;\n    z-index: 9;\n}\n.e_categoryC .p_c_tree>ul>.p_c_item>.p_c_content{\n    left: 0;\n    top: 100%;\n}\n\n.e_categoryC .p_c_mbtn,.e_categoryC .p_c_tree .p_c_mput{\n    display:none;\n}\n"
    },
    {
      "media": "screen and (max-width:768px)",
