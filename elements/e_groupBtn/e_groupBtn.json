{
  "cname": "按钮组",
  "styleId": "e_groupBtn",
  "styleClass": "",
  "tmpl": "<div class=\"${elemId} ${styleClass}\">\n       <div class=\"p_btn\"    bind=\"source-data-templateId|source-data-productId|source-data-coverImage|source-data-retailPrice|source-data-price|source-data-title|source-data-href\" data-source=\"list\"  data-templateId=\"{{templateId}}\" data-productId=\"{{productId}}\" data-coverImage=\"{{coverImage}}\" data-retailPrice=\"{{retailPrice}}\" data-price=\"{{price}}\" data-title=\"{{title}}\" data-href=\"{{href}}\"> \n            {{#if ${showaddCart} }}\n                <a href=\"JavaScript:;\"  class=\"s_button btn  btn-primary btn-sm p_btn_set\"   >{{${i18n.addCart}}}</a>\n        {{/if}}\n            {{#if ${showsoGo} }}\n                <a href=\"JavaScript:;\"  class=\"s_button btn  btn-primary btn-sm p_btn_set\"  >{{${i18n.soGo}}}</a>\n        {{/if}}\n        {{#if ${showcomparison} }}\n                <a href=\"JavaScript:;\"  class=\"s_button btn  btn-primary btn-sm p_btn_set\" data-compar='com'   onclick=\"comparepop(this)\"  >{{${i18n.comparison}}}</a>\n        {{/if}}\n        <div class=\"p_a_btn\"  panel=\"dataList-dataList\" data-ename=\"循环按钮集\">\n                {{#${dataList}}}\n                <span class=\"p_datalistspan p_btn_set\" data-ename=\"循环按钮item\">\n                    \n                    <a href=\"{{$jumpLink linkType linkUrl}}\" target=\"{{linkTarget}}\" class=\"s_button btn  btn-primary btn-sm p_btn p_btn_set\"  panelItem =\"link-link\">\n                    {{buttonName}}\n                    </a>\n                </span>\n            {{/${dataList}}}\n        \n        </div>\n        <div class=\"p_listbtn\" bind=\"arr-productButtonList\" data-ename=\"自定义按钮\">\n        {{#productButtonList}}\n        <span class=\"p_abtn p_btn_set\" data-ename=\"按钮元素\">\n            <a href=\"{{${productButtonList$buttonUrl}}}\"  class=\"s_button btn  btn-primary btn-sm p_btn\" bind=\"href-buttonUrl|data-buttonName\" >{{${productButtonList$buttonName}}}</a>\n        </span>\n            \n        {{/productButtonList}}\n        </div>\n        \n    </div>\n</div>",
  "css": [
    {
      "media": "",
      "cssText": "\n.e_groupBtn .p_a_btn{\n    display: inline-block;\n}\n.e_groupBtn .p_btn_set{\n    display: inline-block;\n    margin-top: 5px;\n}"
    },
    {
      "media": "screen and (max-width:768px)",
      "cssText": ""
    },
    {
      "media": "screen and (min-width: 769px)",
      "cssText": ""
    },
    {
      "media": "screen and (max-width: 1024px) and (min-width: 769px)",
      "cssText": ""
    }
  ],
  "prop": {
    "showImages": true,
    "showaddCart": true,
    "showsoGo": true,
    "showcomparison": true,
    "dataList": [
      {
        "linkUrl": "#",
        "linkType": "5",
        "linkTarget": "",
        "buttonName": "预约申请"
      },
      {
        "linkUrl": "#",
        "linkType": "5",
        "linkTarget": "",
        "buttonName": "样品申请"
      },
      {
        "linkUrl": "#",
        "linkType": "5",
        "linkTarget": "",
        "buttonName": "询价"
      },
      {
        "linkUrl": "#",
        "linkType": "5",
        "linkTarget": "",
        "buttonName": "在线预订"
      },
      {
        "linkUrl": "#",
        "linkType": "5",
        "linkTarget": "",
        "buttonName": "获取报价"
      }
    ]
  },
  "i18n": {
    "zh_CN": {
      "products": "商品",
      "addCart": "加入购物车",
      "soGo": "立即购买",
      "comparison": "对比"
    }
  },
  "datasource": "prop",
  "dataFields": {
    "showImages": "showImages",
    "showaddCart": "showaddCart",
    "showsoGo": "showsoGo",
    "showcomparison": "showcomparison",
    "dataList": "dataList",
    "dataList$linkUrl": "linkUrl",
    "dataList$linkType": "linkType",
    "dataList$linkTarget": "linkTarget",
    "dataList$buttonName": "buttonName"
  },
  "dataManage": {}
}