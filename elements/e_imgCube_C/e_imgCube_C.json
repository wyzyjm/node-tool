  "tmpl": "<div class=\"${elemId} ${styleClass}\">\n    {{#${imgList}}}\n        {{#eq @index 0}}\n\t\t\t<div class=\"p_imgItem \" style=\"width:calc(( 100% - {{../${space}}}px) / 3);height:calc(( 100% - {{../${space}}}px) / 2)\">\n                <!-- 链接 -->\n                {{#if href.value}}\t\n                <a href=\"{{$transferLink href compId}}\" target=\"{{href.target}}\">\n                {{/if}}\n                    <!-- 图片 -->\n                    {{{nLazyImage url title \"{{isLazy}}\"}}}\n                {{#if href.value}}\t\n                </a>\n                {{/if}}\n            </div>      \n\t\t{{/eq}}\n\n        {{#eq @index 1}}\n\t\t\t<div class=\"p_imgItem \" style=\"width:calc(( 100% - {{../${space}}}px) / 3 * 2);height:calc(( 100% - {{../${space}}}px) / 2)\">\n                <!-- 链接 -->\n                {{#if href.value}}\t\n                <a href=\"{{$transferLink href compId}}\" target=\"{{href.target}}\">\n                {{/if}}\n                    <!-- 图片 -->\n                    {{{nLazyImage url title \"{{isLazy}}\"}}}\n                {{#if href.value}}\t\n                </a>\n                {{/if}}\n            </div>\n\t\t{{/eq}}\n        \n        {{#eq @index 2}}\n\t\t\t<div class=\"p_imgItem \" style=\"width:calc(( 100% - {{../${space}}}px) / 3 * 2);height:calc(( 100% - {{../${space}}}px) / 2)\">\n                <!-- 链接 -->\n                {{#if href.value}}\t\n                <a href=\"{{$transferLink href compId}}\" target=\"{{href.target}}\">\n                {{/if}}\n                    <!-- 图片 -->\n                    {{{nLazyImage url title \"{{isLazy}}\"}}}\n                {{#if href.value}}\t\n                </a>\n                {{/if}}\n            </div>\n\t\t{{/eq}} \n\n        {{#eq @index 3}}\n\t\t\t<div class=\"p_imgItem \" style=\"width:calc(( 100% - {{../${space}}}px) / 3);height:calc(( 100% - {{../${space}}}px) / 2)\">\n                <!-- 链接 -->\n                {{#if href.value}}\t\n                <a href=\"{{$transferLink href compId}}\" target=\"{{href.target}}\">\n                {{/if}}\n                    <!-- 图片 -->\n                    {{{nLazyImage url title \"{{isLazy}}\"}}}\n                {{#if href.value}}\t\n                </a>\n                {{/if}}\n            </div>\n\t\t{{/eq}} \n    {{/${imgList}}}\n</div>\n",
  "css": [
    {
      "media": "",
      "cssText": ".e_imgCube_C{\n    width: 100%;\n    max-width: 700px;\n    height: 525px;\n    overflow: hidden;\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: space-between;\n    align-content: space-between;\n}\n\n.e_imgCube_C img {\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n}\n\n.e_imgCube_C .p_imgItem {\n    overflow: hidden;\n}\n"
