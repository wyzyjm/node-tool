{
  "cname": "搜索图集",
  "styleId": "e_imageSearch",
  "styleClass": "s_img",
  "tmpl": "<div class=\"${elemId} ${styleClass}\" needjs=\"true\">\r\n    {{#if ${imgList}}}\r\n        <div class=\"swiper-container p_container\">\r\n            <div class=\"swiper-wrapper\">\r\n                {{#each ${imgList}}}\r\n                <div class=\"swiper-slide\" >\r\n                    <div class=\"p_img\">\r\n                        {{#if href.value}}\r\n                            <a href=\"{{$transferLink href compId}}\" target=\"{{href.target}}\">\r\n                        {{/if}}\r\n                                {{{nLazyImage imageUrl ../title ../setting.isLazy}}}\r\n                        {{#if href.value}}\r\n                            </a>\r\n                        {{/if}}\r\n                    </div>\r\n                </div>\r\n                {{/each}}\r\n            </div>\r\n            <div class=\"js_editor_click swiper-button-next  swiper-button-white p_next\"></div>\r\n            <div class=\"js_editor_click swiper-button-prev swiper-button-white p_prev\"></div>\r\n        </div>\r\n    {{^}}\r\n        {{{nNoDataPrompt ${i18n.noData}}}}\r\n    {{/if}}\r\n</div>",
  "css": [
    {
      "media": "",
      "cssText": "/* .e_imageSearch {\n    height: 200px;\n} */\n.e_imageSearch .p_container {\n    height: 100%;\n}\n\n.e_imageSearch img {\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n}\n.e_imageSearch .swiper-button-next.swiper-button-disabled,\n.e_imageSearch .swiper-button-prev.swiper-button-disabled {\n    display: none;\n}\n"
    },
