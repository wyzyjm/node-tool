<div class="e_loop_E s_list" needjs="true">
     <div class="swiper-container">
        {{#if prop.dataList}}
            <div class="swiper-wrapper">
                {{#prop.dataList}}
                <div class="swiper-slide">
                    <div class="slide-inner">
                         <div class="cbox-${id} p_template"></div>
                    </div>
                </div>
                {{/prop.dataList}}
            </div>   
             <div class="swiper-pagination swiper-pagination-white"></div>
            <!-- 如果需要导航按钮 -->
            <div class="js_editor_click swiper-button-prev swiper-button-white"></div>
            <div class="js_editor_click swiper-button-next swiper-button-white"></div>
        {{/if}}
    </div>
</div>