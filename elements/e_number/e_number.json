{
  "cname": "数字输入框",
  "styleId": "e_number",
  "styleClass": "s_form1",
  "type": "form",
  "putSubForm": true,
  "tmpl": "<div class=\"${elemId} ${styleClass} form-group {{#eq ${state} 'hidden'}}formHide{{/eq}}\" needjs=\"true\" {{#if ${validate}.required.value}}required{{/if}}>\n    {{#if ${showLabel}}}\n    <label class=\"p_label\">\n        <span class=\"s_label\">{{${label}}}</span>\n    </label>\n    {{/if}}\n    {{#eq ${state} 'readOnly'}}\n    <p class=\"s_input p_formValue\">{{${value}}}</p>\n    {{/eq}}\n    <div class=\"p_number {{#eq ${state} 'readOnly'}}hide{{/eq}}\">\n        <div class=\"input-group\">\n            <input type=\"number\" class=\"form-control s_form-control s_input p_input\" name=\"${elemId}\" \n                {{#if ${showPlaceholder}}}placeholder=\"{{${placeholder}}}\"{{/if}} \n                {{#if ${value}}}value=\"{{${value}}}\"{{/if}} \n                {{#eq ${state} 'disabled'}}disabled{{/eq}}\n            >\n            <div class=\"input-group-append\">\n                {{#if ${unit}}}\n                <span class=\"s_other p_unit\">\n                {{${unit}}}\n                </span>\n                {{/if}}\n            </div>\n            <div class=\"invalid-feedback\"></div>\n        </div>\n    </div>\n</div>",
  "css": [
    {
      "media": "",
      "cssText": ".e_number .p_unit{\n    line-height: 36px;\n    padding:0 10px;\n}\n.e_number .p_input{\n    -moz-appearance: textfield;\n}\n.e_number .p_input::-webkit-outer-spin-button,\n.e_number .p_input::-webkit-inner-spin-button {\n    -webkit-appearance: none;\n}"
    },
    {
      "media": "screen and (max-width:768px)",
      "cssText": ""
    },
    {
      "media": "screen and (min-width: 769px)",
      "cssText": ""
    },
    {
      "media": "screen and (max-width: 1024px) and (min-width: 769px)",
      "cssText": ""
    }
  ],
  "prop": {
    "showLabel": true,
    "label": "数字输入框",
    "showPlaceholder": true,
    "placeholder": "请输入...",
    "value": "",
    "state": "normal",
    "decimal": 2,
    "unit": "",
    "initType": 1,
    "datasource": {},
    "ddItem": {},
    "validate": {
      "required": {
        "state": true,
        "value": true,
        "msg": "内容不能为空"
      },
      "min": {
        "state": false,
        "value": 10,
        "msg": "您输入的内容最小值为%len"
      },
      "max": {
        "state": false,
        "value": 100,
        "msg": "您输入的内容最大值为%len"
      },
      "custom": {
        "state": false,
        "reg": "",
        "msg": ""
      }
    }
  },
  "i18n": {
    "zh_CN": {},
    "en": {},
    "de": {},
    "fr": {},
    "ru": {},
    "pt": {},
    "ko": {},
    "ja": {},
    "es": {},
    "ar": {},
    "it": {},
    "bn": {},
    "da": {},
    "idn": {},
    "th": {},
    "vi": {},
    "tr": {},
    "nl": {},
    "pl": {},
    "zh_HN": {}
  },
  "datasource": "prop"
}