{
  "cname": "评分、评价",
  "styleId": "e_score",
  "styleClass": "s_form1",
  "type": "form",
  "putSubForm": true,
  "tmpl": "<div class=\"${elemId} ${styleClass} form-group\" needjs=\"true\" {{#if ${validate}.required.value}}required{{/if}}>\n    {{#if ${showLabel}}}\n    <label class=\"p_label\">\n        <span class=\"s_label\">{{${label}}}</span>\n    </label>\n    {{/if}}\n    <div class=\"clearfix p_input\">\n        <div class=\"p_score\"></div>\n        <div class=\"p_hint s_other\"></div>\n        <input type=\"hidden\" name=\"${elemId}\" value=\"{{#if score}}{{score}}/{{number}}{{/if}}\">\n    </div>\n    <div class=\"invalid-feedback\"></div>\n</div>\n",
  "css": [
    {
      "media": "",
      "cssText": ".e_score{\n\n}\n.e_score .p_score{\n    float: left;\n    margin-right: 10px;\n}\n.e_score .p_hint{\n    float: left;\n    line-height: 24px;\n}"
    },
    {
      "media": "screen and (max-width:768px)",
      "cssText": ""
    },
    {
      "media": "screen and (min-width: 769px)",
      "cssText": ""
    },
    {
      "media": "screen and (max-width: 1024px) and (min-width: 769px)",
      "cssText": ""
    }
  ],
  "prop": {
    "showLabel": true,
    "label": "评分评价",
    "showPlaceholder": true,
    "placeholder": "请选择评分",
    "type": 0,
    "number": 5,
    "score": 0,
    "half": true,
    "prompt": [
      {
        "from": 1,
        "to": 1,
        "label": "非常不满意"
      },
      {
        "from": 2,
        "to": 2,
        "label": "不满意"
      },
      {
        "from": 3,
        "to": 3,
        "label": "一般"
      },
      {
        "from": 4,
        "to": 4,
        "label": "满意"
      },
      {
        "from": 5,
        "to": 5,
        "label": "非常满意"
      }
    ],
    "validate": {
      "required": {
        "state": true,
        "value": true,
        "msg": "内容不能为空"
      }
    }
  },
  "i18n": {
    "zh_CN": {}
  },
  "datasource": "prop"
}