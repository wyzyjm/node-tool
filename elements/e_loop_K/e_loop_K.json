  "css": [
    {
      "media": "",
      "cssText": ".e_loop_K table {\n    border-collapse: collapse;\n    width: 100%;\n}\n.e_loop_K table th,\n.e_loop_K table td {\n    border: 1px solid #ccc;\n}\n.e_loop_K table th {\n    text-align: center;\n}\n.e_loop_K table tbody td{\n    position: relative;\n    height: 120px;\n}\n.e_loop_K .p_icon,\n.e_loop_K .p_screen {\n    position: relative;\n    cursor: pointer;\n}\n.e_loop_K .p_screen .p_screenData {\n    display: none;\n    position: absolute;\n    left: 22px;\n    top: 12px;\n    width: 100px;\n    border-radius: 4px;\n    box-shadow: 0px 2px 8px 0px rgba(22, 45, 61, 0.3);\n    background: #fff;\n    z-index: 9999;\n}\n.e_loop_K .p_screen .p_screenData label {\n    float: left;\n    width: 100%;\n    margin: 3px 0;\n    text-align: left;\n}\n.e_loop_K .p_screen .p_optDta{\n    padding: 5px;\n    overflow: hidden;\n}\n.e_loop_K .p_screen .p_clear {\n    background: #4d90cb;\n    color: #fff;\n    text-align: center;\n    height: 30px;\n    line-height: 30px;\n    border-radius: 2px;\n    padding: 0 4px;\n    cursor: pointer;\n}\n.e_loop_K .p_loopitem{\n    min-width: 50px;\n}"
