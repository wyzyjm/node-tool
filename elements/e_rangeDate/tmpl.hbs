<div class="e_rangeDate form-group" needjs="true">
    {{#if prop.showLabel}}
    <label class="p_label">
        <span>{{prop.label}}</span>
        {{#if prop.validate.required.value}}<span class="text-danger p_required">*</span>{{/if}}
    </label>
    {{/if}}
    <div class="j_date form-control clearfix p_range">
        <input type="text" name="${elemId}-start" {{#eq prop.showPlaceholder 'word'}}placeholder="{{prop.startPlaceholder}}"{{/eq}} class="p_start">
        <span class="p_fenge">â€”</span>
        <input type="text" name="${elemId}-end" {{#eq prop.showPlaceholder 'word'}}placeholder="{{prop.endPlaceholder}}"{{/eq}} class="p_end">
    </div>
    <div class="invalid-feedback"></div>
    <input type="hidden" name="propJson" value='{{{propJson}}}'>
</div>