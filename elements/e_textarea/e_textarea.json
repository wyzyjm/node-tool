{
  "cname": "多行文本",
  "styleId": "e_textarea",
  "styleClass": "s_form1",
  "type": "form",
  "putSubForm": true,
  "tmpl": "<div class=\"${elemId} ${styleClass} form-group {{#eq ${state} 'hidden'}}formHide{{/eq}}\" needjs=\"true\">\n    {{#if ${showLabel}}}\n    <label class=\"p_label\">\n        <span class=\"s_label\">{{${label}}}</span>\n        {{#if ${validate}.required.value}}<span class=\"text-danger p_required\">*</span>{{/if}}\n    </label>\n    {{/if}}\n    {{#eq ${state} 'readOnly'}}\n    <p class=\"s_input p_formValue\">{{${value}}}</p>\n    {{/eq}}\n    <div class=\"{{#eq ${state} 'readOnly'}}hide{{/eq}}\">\n        <textarea class=\"form-control s_form-control s_input p_input\" rows=\"3\" name=\"${elemId}\" \n            {{#if ${showPlaceholder}}}placeholder=\"{{${placeholder}}}\"{{/if}} \n            {{#eq ${state} 'disabled'}}disabled{{/eq}} \n        >{{#if ${value}}}{{${value}}}{{/if}}</textarea>\n        <div class=\"invalid-feedback\"></div>\n    </div>\n</div>\n",
  "css": [
    {
      "media": "",
      "cssText": ".e_textarea{\n\n}\n.e_textarea  .p_required{\n    vertical-align: middle;\n    font-size: 1.4em;\n    line-height: 100%;\n    position: relative;\n    font-weight: bold;\n    top: 0.15em;\n    padding-left: 5px;\n}"
    },
    {
      "media": "screen and (max-width:768px)",
      "cssText": ""
    },
    {
      "media": "screen and (min-width: 769px)",
      "cssText": ""
    },
    {
      "media": "screen and (max-width: 1024px) and (min-width: 769px)",
      "cssText": ""
    }
  ],
  "prop": {
    "showLabel": true,
    "label": "多行文本",
    "showPlaceholder": true,
    "placeholder": "请输入...",
    "value": "",
    "state": "normal",
    "validate": {
      "required": {
        "state": true,
        "value": true,
        "msg": "内容不能为空"
      },
      "minLength": {
        "state": false,
        "value": 5,
        "msg": "您输入的内容不能少于%len个字符"
      },
      "maxLength": {
        "state": false,
        "value": 128,
        "msg": "您输入的内容不能多于%len个字符"
      },
      "custom": {
        "state": false,
        "reg": "",
        "msg": ""
      }
    }
  },
  "i18n": {
    "zh_CN": {}
  },
  "datasource": "prop",
  "dataFields": {}
}