{
  "cname": "产品规格",
  "styleId": "e_specification",
  "styleClass": "",
  "tmpl": "<ul class=\"${elemId} ${styleClass}\">\r\n    {{#${specList}}}\r\n        <li class=\"p_item\">\r\n            <!--规格名称-->\r\n            <span class=\"p_specName\"> {{specName}} </span>\r\n            <!--规格值-->\r\n            <ul class=\"p_valueUl\">\r\n                {{#productSpecValues}}\r\n                <li class=\"itemValue\"> \r\n                    {{#if ../imageFlag}}\r\n                        <!--图片类型-->\r\n                        {{{lazyImage img \"class='specValueI' title='{{name}}' alt='{{name}}' \" 'true'}}}\r\n                    {{^}}\r\n                        <!--文字类型-->\r\n                        <span class=\"specValueT\">{{name}}</span>\r\n                    {{/if}}\r\n                </li>\r\n                {{/productSpecValues}}\r\n            </ul>\r\n        </li>\r\n    {{/${specList}}}\r\n    <!--js传值-->\r\n    <!--   <input type=\"hidden\" name=\"propJson\" value=\"{{{propJson}}}\">  -->\r\n</ul>",
  "css": [
    {
      "media": "",
      "cssText": ".e_specification{\r\n\r\n}\r\n.e_specification img{\r\n    width: 100%;\r\n    height: 100%;\r\n    object-fit: cover;\r\n}\r\n.e_specification .p_item{\r\n    padding-bottom: 10px;\r\n    overflow: hidden;\r\n}\r\n.e_specification .p_specName {\r\n   float: left;\r\n   height: 30px;\r\n   line-height: 30px;\r\n}\r\n.e_specification .p_valueUl {\r\n    margin-left: 40px;\r\n    overflow: hidden;\r\n}\r\n.e_specification .itemValue {\r\n    float: left;\r\n    margin-right: 5px;\r\n    margin-right: 5px;\r\n    margin-bottom: 5px;\r\n    overflow: hidden;\r\n}\r\n.e_specification .specValueT {\r\n    display: block;\r\n    padding: 3px 16px;\r\n    text-align: center;\r\n    border: 1px solid #ccc;\r\n}\r\n.e_specification .specValueI {\r\n    width: 46px;\r\n    height: 46px;\r\n}"
    },
    {
      "media": "screen and (max-width:768px)",
      "cssText": ".e_specification .p_specName {\r\n    display: block;\r\n    float: none\r\n }\r\n .e_specification .p_valueUl {\r\n    margin-left: 0;\r\n}"
    },
    {
      "media": "screen and (min-width: 769px)",
      "cssText": ""
    },
    {
      "media": "screen and (max-width: 1024px) and (min-width: 769px)",
      "cssText": ""
    }
  ],
