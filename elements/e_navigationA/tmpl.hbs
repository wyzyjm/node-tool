<div class="e_navigationA" needjs="true">
    {{#if prop.dataList}}
    <p class="p_iconBox">
        {{{prop.menu}}}
        {{{prop.close}}}
    </p>
    <ul class="bg-white p_level1Box">
        {{#each prop.dataList}}
        <li class="border-bottom p_level1Item">
            <p class="p_menuItem">
                <a href="{{#if href}}{{href}}{{^}}javascript:;{{/if}}" target="{{#eq target 'BLANk'}}_blank{{/eq}}">
                    <span>{{name}}</span>
                    {{#if children}}{{{../prop.arrow}}}{{/if}}
                </a>
            </p>
            {{#if children}}
            <ul class="bg-white p_level2Box">
                {{#each children}}
                <li class="p_level2Item">
                    <p class="p_menuItem">
                        <a href="{{#if href}}{{href}}{{^}}javascript:;{{/if}}" target="{{#eq target 'BLANk'}}_blank{{/eq}}">
                            <span>{{name}}</span>
                            {{#if children}}{{{../../prop.arrow}}}{{/if}}
                        </a>
                    </p>
                    {{#if children}}
                    <ul class="bg-white p_level3Box">
                        {{#each children}}
                        <li class="p_level3Item">
                            <p class="p_menuItem">
                                <a href="{{#if href}}{{href}}{{^}}javascript:;{{/if}}" target="{{#eq target 'BLANk'}}_blank{{/eq}}">
                                    <span>{{name}}</span>
                                </a>
                            </p>
                        </li>
                        {{/each}}
                    </ul>
                    {{/if}}
                </li>
                {{/each}}
            </ul>
            {{/if}}
        </li>
        {{/each}}
    </ul>
    {{^}}
    {{{nNoDataPrompt i18n.noData}}}
    {{/if}}
</div>

