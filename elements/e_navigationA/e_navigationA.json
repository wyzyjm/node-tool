  "css": [
    {
      "media": "",
      "cssText": ".e_navigationA .p_level2Box, \n.e_navigationA .p_level3Box{\n    display: none;\n    height: auto;\n}\n.e_navigationA p{\n    height: 50px;\n    line-height: 50px;\n}\n.e_navigationA .p_iconBox{\n    height: 57px;\n    padding: 10px;\n}\n.e_navigationA .p_openIcon, \n.e_navigationA .p_closeIcon{\n    width: 36px;\n    height: 36px;\n}\n"
    },
    {
      "media": "screen and (max-width:768px)",
      "cssText": ".e_navigationA .p_level1Box{\n    padding: 0 20px;\n    display: none;\n    height: calc(100vh - 57px);\n}\n.e_navigationA .p_level2Box, \n.e_navigationA .p_level3Box{\n    padding-left: 20px;\n}\n.e_navigationA .p_iconBox{\n    display: block;\n}\n.e_navigationA .p_closeIcon{\n    float: right;\n    display: none;\n}\n.e_navigationA .p_jtIcon{\n    float: right;\n    margin: 15px 0;\n    transition: 0.5s;\n    color: #cdcdcd;\n    transform: rotate(90deg);\n}\n.e_navigationA .p_jtHover {\n    transition: 0.5s;\n    transform: rotate(180deg);\n}"
    },
    {
      "media": "screen and (min-width: 769px)",
      "cssText": ".e_navigationA .p_level1Box{\n    display: flex;\n}\n.e_navigationA .p_level1Item{\n    width: 160px;\n    padding: 0 6px;\n}\n.e_navigationA .p_level2Item, \n.e_navigationA .p_level3Item{\n    width: 200px;\n    padding: 0 6px;\n}\n.e_navigationA .p_level1Item{\n    position: relative;\n    border: none;\n}\n.e_navigationA .p_level2Box{\n    width: 200px;\n    position: absolute;\n    left: 0;\n    top: 40px;\n    box-shadow: -5px 5px 20px rgb(236, 233, 233);\n}\n.e_navigationA .p_level2Item{\n    position: relative;\n}\n.e_navigationA .p_level3Box{\n    width: 200px;\n    position: absolute;\n    top: 0;\n    left: 200px;\n    z-index: 9;\n    box-shadow: 5px 5px 20px rgb(236, 233, 233);\n}\n.e_navigationA p:hover{\n    color: #409EFF;\n}\n.e_navigationA .p_jtIcon{\n    /* transition: 0.5s; */\n    transform: rotate(180deg);\n    color: #cdcdcd;\n}\n.e_navigationA .p_level1Item:hover .p_jtIcon {\n    transition: 0.5s;\n    color: #409eff !important;\n    /* transform: rotate(180deg); */\n}\n.e_navigationA .p_level2Item .p_jtIcon{\n    display: none;\n}\n\n.e_navigationA .p_iconBox{\n    display: none;\n}"
    },
    {
      "media": "screen and (max-width: 1024px) and (min-width: 769px)",
      "cssText": ""
    }
  ],
