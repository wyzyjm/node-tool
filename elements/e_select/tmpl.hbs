<div class="e_select s_form1 form-group {{#eq prop.state 'hidden'}}formHide{{/eq}}" needjs="true" {{#if prop.validate.required.value}}required{{/if}}>
    {{#if prop.showLabel}}
    <label class="p_label">
        <span class="s_label">{{prop.label}}</span>
    </label>
    {{/if}}
    {{#eq prop.state 'readOnly'}}
    <p class="p_formValue s_input"></p>
    {{/eq}}
    <div class="{{#eq prop.state 'readOnly'}}hide{{/eq}}">
        <select class="form-control s_form-control s_input p_input" name="${elemId}" {{#eq prop.state 'disabled'}}disabled{{/eq}} >
            {{#if prop.showPlaceholder}}
            <option value="">{{prop.placeholder}}</option>
            {{/if}}
            {{#prop.options}}
            <option value="{{value}}">{{label}}</option>
            {{/prop.options}}
        </select>
        <div class="invalid-feedback"></div>
    </div>
</div>
