{
  "cname": "下拉选项",
  "styleId": "e_select",
  "styleClass": "s_form1",
  "type": "form",
  "putSubForm": true,
  "tmpl": "<div class=\"${elemId} ${styleClass} form-group {{#eq ${state} 'hidden'}}formHide{{/eq}}\" needjs=\"true\">\n    {{#if ${showLabel}}}\n    <label class=\"p_label\">\n        <span class=\"s_label\">{{${label}}}</span>\n        {{#if ${validate}.required.value}}<span class=\"text-danger p_required\">*</span>{{/if}}\n    </label>\n    {{/if}}\n    {{#eq ${state} 'readOnly'}}\n    <p class=\"p_formValue s_input\"></p>\n    {{/eq}}\n    <div class=\"{{#eq ${state} 'readOnly'}}hide{{/eq}}\">\n        <select class=\"form-control s_form-control s_input p_input\" name=\"${elemId}\" {{#eq ${state} 'disabled'}}disabled{{/eq}} >\n            {{#if ${showPlaceholder}}}\n            <option value=\"\">{{${placeholder}}}</option>\n            {{/if}}\n            {{#${options}}}\n            <option value=\"{{value}}\">{{label}}</option>\n            {{/${options}}}\n        </select>\n        <div class=\"invalid-feedback\"></div>\n    </div>\n</div>\n",
  "css": [
    {
      "media": "",
      "cssText": ".e_select{\n\n}\n.e_select  .p_required{\n    vertical-align: middle;\n    font-size: 1.4em;\n    line-height: 100%;\n    position: relative;\n    font-weight: bold;\n    top: 0.15em;\n    padding-left: 5px;\n}"
    },
    {
      "media": "screen and (max-width:768px)",
      "cssText": ""
    },
    {
      "media": "screen and (min-width: 769px)",
      "cssText": ""
    },
    {
      "media": "screen and (max-width: 1024px) and (min-width: 769px)",
      "cssText": ""
    }
  ],
  "prop": {
    "showLabel": true,
    "label": "下拉选择",
    "showPlaceholder": true,
    "placeholder": "请选择",
    "state": "normal",
    "options": [
      {
        "label": "已婚",
        "value": "1"
      },
      {
        "select": true,
        "label": "未婚",
        "value": "2"
      },
      {
        "label": "未知",
        "value": "3"
      }
    ],
    "validate": {
      "required": {
        "state": true,
        "value": true,
        "msg": "内容不能为空"
      }
    }
  },
  "i18n": {
    "zh_CN": {}
  },
  "datasource": "prop",
  "dataFields": {
    "options$label": "label",
    "options$value": "value"
  },
  "scope": true
}