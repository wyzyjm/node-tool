      "media": "",
      "cssText": ".e_imageList{\n    height: 600px;\n}\n.e_imageList .p_img{\n    width: 100%;\n    height: 100%;\n    background: #000;\n}\n.e_imageList .p_img img{\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n}\n.e_imageList .custom-bullet{\n    background:#ccc;\n}\n.e_imageList .swiper-slide img{\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n}\n.e_imageList .gallery-top {\n    height: 80%;\n    width: 100%;\n}\n.e_imageList .gallery-thumbs {\n    height: 20%;\n    box-sizing: border-box;\n    padding-top: 5px;\n}\n.e_imageList .gallery-thumbs .swiper-slide {\n    width: 25%;\n    height: 100%;\n    opacity: 0.6;\n}\n.e_imageList .gallery-thumbs .swiper-slide img{\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n}\n.e_imageList .gallery-thumbs .swiper-slide-thumb-active {\n    opacity: 1;\n}\n"
