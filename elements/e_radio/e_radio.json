{
  "cname": "单选项",
  "styleId": "e_radio",
  "styleClass": "s_form1",
  "type": "form",
  "putSubForm": true,
  "tmpl": "<fieldset class=\"${elemId} ${styleClass} form-group {{#eq ${state} 'hidden'}}formHide{{/eq}}\" needjs=\"true\" {{#if ${validate}.required.value}}required{{/if}}>\n    {{#if ${showLabel}}}\n    <legend class=\"col-form-label p_label\">\n        <span class=\"s_label\">{{${label}}}</span>\n    </legend>\n    {{/if}}\n    {{#eq ${state} 'readOnly'}}\n    <p class=\"s_input p_formValue\"></p>\n    {{/eq}}\n    <div class=\"{{#eq ${state} 'readOnly'}}hide{{/eq}}\">\n        <div class=\"p_input\">\n            {{#${options}}}\n            <div class=\"form-check form-check-inline  p_radioItem\">\n                <input class=\"form-check-input s_form-control\" type=\"radio\" name=\"${elemId}\" id=\"${elemId}-{{@index}}\" value=\"{{value}}\" {{#if select}}checked{{/if}} {{#eq ../${state} 'disabled'}}disabled{{/eq}}>\n                <label class=\"form-check-label s_other\" for=\"${elemId}-{{@index}}\">{{label}}</label>\n            </div>\n            {{/${options}}}\n        </div>\n        <div class=\"invalid-feedback\"></div>\n    </div>\n</fieldset>",
  "css": [
    {
      "media": "",
      "cssText": ""
    },
    {
      "media": "screen and (max-width:768px)",
      "cssText": ""
    },
    {
      "media": "screen and (min-width: 769px)",
      "cssText": ""
    },
    {
      "media": "screen and (max-width: 1024px) and (min-width: 769px)",
      "cssText": ""
    }
  ],
  "scope": true,
  "prop": {
    "showLabel": true,
    "label": "单选项",
    "state": "normal",
    "options": [
      {
        "label": "已婚",
        "value": "1"
      },
      {
        "select": true,
        "label": "未婚",
        "value": "2"
      },
      {
        "label": "未知",
        "value": "3"
      }
    ],
    "datasourceList": [],
    "ddItemList": [],
    "validate": {
      "required": {
        "state": true,
        "value": true,
        "msg": "内容不能为空"
      }
    }
  },
